<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:ags="http://www.purl.org/agmes/1.1/"
	xmlns:aida="http://www.idmlinitiative.org/resources/dtds/AIDA22.xsd" xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dwc="http://rs.tdwg.org/dwc/terms/" xmlns:fi="http://www.fao.org/fi/figis/devcon/" targetNamespace="http://www.fao.org/fi/figis/devcon/"
	elementFormDefault="qualified">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd" />
	<xs:import namespace="http://purl.org/dc/elements/1.1/" schemaLocation="dc.xsd" />
	<xs:import namespace="http://rs.tdwg.org/dwc/terms/" schemaLocation="dwc.xsd" />
	<xs:import namespace="http://purl.org/dc/terms/" schemaLocation="dcterms.xsd" />
	<xs:import namespace="http://www.purl.org/agmes/1.1/" schemaLocation="ags.xsd" />
	<xs:import namespace="http://www.idmlinitiative.org/resources/dtds/AIDA22.xsd" schemaLocation="aida.xsd" />
	<xs:annotation>
		<xs:documentation>
			<Title>Fisheries Metadata Element Set (FIMES)</Title>
			<Description>To manage information in a systematic and cost-effective way, FIGIS has built a metadata level (a topic
				tree) for each information
				domain, providing a standard template for the display of Web page content. The Fisheries
				Metadata Element Set (FIMES) describes this meta level
				which is based on XML language.</Description>
		</xs:documentation>
	</xs:annotation>
	<xs:group name="indexEnt">
		<xs:choice>
			<xs:element ref="fi:Sources" />
			<xs:element ref="fi:Bibliography" />
			<xs:element ref="fi:RelatedResources" />
		</xs:choice>
	</xs:group>
	<xs:group name="textEnt">
		<xs:choice>
			<xs:element ref="fi:Text" />
			<xs:element ref="fi:Image" />
			<xs:element ref="fi:Table" />
			<xs:element ref="fi:Chart" />
			<xs:element ref="fi:KnowledgeObjRef" />
			<xs:element ref="fi:Contacts" />
			<xs:element ref="fi:FIGISSearch" />
			<xs:element ref="fi:FIGISCondLink" />
			<xs:element ref="fi:FIGISCondLinkSet" />
		</xs:choice>
	</xs:group>
	<xs:group name="textEntNoKOR">
		<xs:choice>
			<xs:element ref="fi:Text" />
			<xs:element ref="fi:Image" />
			<xs:element ref="fi:Table" />
			<xs:element ref="fi:Chart" />
		</xs:choice>
	</xs:group>
	<xs:group name="textEntNoTitle">
		<xs:choice>
			<xs:element ref="fi:Text" />
			<xs:element ref="fi:Image" />
			<xs:element ref="fi:Table" />
		</xs:choice>
	</xs:group>
	<xs:group name="textEntMandTitle">
		<xs:sequence>
			<xs:element ref="dc:Title" />
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Text" />
				<xs:element ref="fi:Image" />
				<xs:element ref="fi:Table" />
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:attributeGroup name="ageType">
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="First_maturity" />
					<xs:enumeration value="Maximum" />
					<xs:enumeration value="Infinity" />
					<xs:enumeration value="Other_type" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="areaUnit">
		<xs:attribute name="Unit" default="sq_km">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="km" />
					<xs:enumeration value="sq_km" />
					<xs:enumeration value="mile" />
					<xs:enumeration value="sq_mile" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="byType">
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="By_species" />
					<xs:enumeration value="By_area" />
					<xs:enumeration value="By_environment" />
					<xs:enumeration value="By_culture_system" />
					<xs:enumeration value="By_farming_system" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="countryEnt">
		<xs:attribute name="country">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[A-Z]{3,}" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="environEnt">
		<xs:attribute name="Environ">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="All" />
					<xs:enumeration value="Marine" />
					<xs:enumeration value="Brackish" />
					<xs:enumeration value="Freshwater" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="legalStatusEnt">
		<xs:attribute name="LegalStatus">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Enforced" />
					<xs:enumeration value="Signed" />
					<xs:enumeration value="Working_proposal" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="lengthUnit">
		<xs:attribute name="Unit" default="metres">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="km" />
					<xs:enumeration value="metres" />
					<xs:enumeration value="cm" />
					<xs:enumeration value="inches" />
					<xs:enumeration value="feet" />
					<xs:enumeration value="yards" />
					<xs:enumeration value="miles" />
					<xs:enumeration value="fathoms" />
					<xs:enumeration value="leagues" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="monetaryScale">
		<xs:attribute name="Currency">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[A-Z]{3,}" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Scale">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="One" />
					<xs:enumeration value="Ten" />
					<xs:enumeration value="Hundred" />
					<xs:enumeration value="Thousand" />
					<xs:enumeration value="Million" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="sectorEnt">
		<xs:attribute name="Sector">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Fishery_and_aquaculture" />
					<xs:enumeration value="Aquaculture" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="speciesAtt">
		<xs:attribute name="OtherType" type="xs:string" />
		<xs:attribute name="OtherUnit" type="xs:string" />
		<xs:attribute name="Land" type="xs:string" />
		<xs:attribute name="Time" type="xs:string" />
	</xs:attributeGroup>
	<xs:attributeGroup name="statusEnt">
		<xs:attribute name="Status" type="xs:integer" use="optional" />
	</xs:attributeGroup>
	<xs:attributeGroup name="volumeUnit">
		<xs:attribute name="Unit" default="litre">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="cu_m" />
					<xs:enumeration value="cu_ft" />
					<xs:enumeration value="litre" />
					<xs:enumeration value="cc" />
					<xs:enumeration value="gallon" />
					<xs:enumeration value="oz" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="weightUnit">
		<xs:attribute name="Unit" default="kg">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="kg" />
					<xs:enumeration value="g" />
					<xs:enumeration value="lb" />
					<xs:enumeration value="piece" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="code">
		<xs:attribute name="Code" type="xs:string" use="required" />
	</xs:attributeGroup>
	<xs:attributeGroup name="asfis">
		<xs:attribute name="CodeSystemType" default="ASFIS">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="ASFIS" />
					<xs:enumeration value="SIDP" />
					<xs:enumeration value="FishFinder" />
					<xs:enumeration value="Eschmeyer" />
					<xs:enumeration value="Other" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="MethodType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Technical_measures" />
			<xs:enumeration value="Input_controls" />
			<xs:enumeration value="Output_controls" />
			<xs:enumeration value="Economic_incentives" />
			<xs:enumeration value="Monitoring" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReferenceYear">
		<xs:annotation>
			<xs:documentation>
				<Title>Reference year</Title>
				<Description>"The year for which the status of the target object (e.g. Marine Resource, Fishery...) has been
					evaluated" (FIRMS TWG2, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{4,}[a-zA-Z]?" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReportingYear">
		<xs:annotation>
			<xs:documentation>
				<Title>Reporting year</Title>
				<Description>"The year in which the scientific meeting (or equivalent scientific validation process) reviewed the
					status of the target object
					(e.g. Marine Resource, Fishery...) pertaining to the fact sheet" (FIRMS TWG2, 2008).</Description>
				<Usage>Year, eventually followed by an index (e.g. 1999a), attached to the observation reporting status and trends
					on a Marine resource or
					Fishery. It is used in the construction of the Title of the observation. Reporting Year
					should not be confused with the Year of Publication nor
					with the Reference Year.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{4,}[a-zA-Z]?" />
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AbundanceLevel">
		<xs:annotation>
			<xs:documentation>
				<Title>Abundance Level</Title>
				<Description>"Degree of plentifulness. The total number of fish in a population or on a fishing ground. Can be
					measured in absolute or relative
					terms." (FAO Fisheries Glossary)</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:StandardAbundanceLevel" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Type" use="optional" default="Unspecified">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="CCAMLR" />
						<xs:enumeration value="CCSBT" />
						<xs:enumeration value="CECAF" />
						<xs:enumeration value="EUROSTAT" />
						<xs:enumeration value="FAO" />
						<xs:enumeration value="FIRMS" />
						<xs:enumeration value="GFCM" />
						<xs:enumeration value="IATTC" />
						<xs:enumeration value="ICCAT" />
						<xs:enumeration value="ICES" />
						<xs:enumeration value="IOTC" />
						<xs:enumeration value="NAFO" />
						<xs:enumeration value="NEAFC" />
						<xs:enumeration value="SEAFDEC" />
						<xs:enumeration value="SEAFO" />
						<xs:enumeration value="WECAFC" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FreeValue" use="optional" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Access">
		<xs:annotation>
			<xs:documentation>
				<Title>SiteIdentification</Title>
				<Description>contains information about procedures involved in identifying a suitable site in which to engage in
					aquaculture.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Lease" />
				<xs:element ref="fi:Legislation" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:TermsAndConditions" />
				<xs:element ref="fi:Procedures" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AccuracyIndicator">
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Acronym">
		<xs:annotation>
			<xs:documentation>
				<Title>Acronym</Title>
				<Description>contains the abbreviated name of an object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Action">
		<xs:annotation>
			<xs:documentation>
				<Title>Action</Title>
				<Description>contains information about Fisheries actions.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ActionPlan">
		<xs:annotation>
			<xs:documentation>
				<Title>Plan of Action</Title>
				<Description>means a management system plan of action applied to a vesseltype, geartype, fishery, fishing technique,
					stock, resource, managment
					unit, species or species group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:International" />
				<xs:element ref="fi:Regional" />
				<xs:element ref="fi:Local" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AddInfo">
		<xs:annotation>
			<xs:documentation>
				<Title>Additional Information</Title>
				<Description>contains additional information related to an object that is deemed of secondary importance.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Factsheet" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="General_information" />
						<xs:enumeration value="Additional_information" />
						<xs:enumeration value="Note" />
						<xs:enumeration value="Remarks" />
						<xs:enumeration value="Footnote" />
						<xs:enumeration value="Endnote" />
						<xs:enumeration value="Popup" />
						<xs:enumeration value="Expand" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AdditionalRefData">
		<xs:annotation>
			<xs:documentation>
				<Title>Additional Reference Data</Title>
				<Description>contains data found in a Ref block that is not part of the RTMS.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dcterms:Alternative" />
			</xs:choice>
			<xs:attribute name="Style">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Full" />
						<xs:enumeration value="NoImage" />
						<xs:enumeration value="Link" />
						<xs:enumeration value="Icon" />
						<xs:enumeration value="Expand" />
						<xs:enumeration value="More" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Layout">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Block" />
						<xs:enumeration value="Bullet" />
						<xs:enumeration value="Indent" />
						<xs:enumeration value="BulletAndIndent" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Address">
		<xs:annotation>
			<xs:documentation>
				<Title>PostalAddress Line 1</Title>
				<Description> fi:AddrLine1 contains the first line of the postal fi:PostalAddress of a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="LineNumber" type="xs:string" use="required" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Advice">
		<xs:annotation>
			<xs:documentation>
				<Title>Management advice</Title>
				<Description>"Management advice are those recommendations generally evolving from the assessment addressed to
					management authorities. It evolves
					from biological scientific advice (found under assessment results) but takes into
					account a broader spectrum of issues. Advice may be proposed
					under different management options, each one including
					a risk assessment element" (FIGIS-FIRMS 2002 workshop).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Age">
		<xs:annotation>
			<xs:documentation>
				<Title>Age</Title>
				<Description>Age is a container for information directly related to the Age of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Min" />
				<xs:element ref="fi:Max" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attributeGroup ref="fi:ageType" />
			<xs:attribute name="Unit" default="Year">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Year" />
						<xs:enumeration value="Other_unit" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:speciesAtt" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Aggregation">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Aggregation" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AidOrInvestments">
		<xs:annotation>
			<xs:documentation>
				<Title>Aid or Investments</Title>
				<Description>On-going or planned projects, with their objectives, time scales, institutions involved, expected
					outcomes for the fishery and
					aquaculture sectors. For management systems, investments and subsidies in fisheries
					with reference to management systems to which these
					investments/subsidies apply.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:OrgOverview" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ApproachEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Approach</Title>
				<Description>is a single instance of an approach used by a management system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="dc:Title" />
					<xs:group ref="fi:textEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Approaches">
		<xs:annotation>
			<xs:documentation>
				<Title>Approaches</Title>
				<Description>are the approaches used by a management system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ApproachEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AqRes">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic resource</Title>
				<Description>According to the FAO Fisheries Glossary, "Biotic element of the aquatic ecosystem, including genetic
					resources, organisms or parts
					thereof, populations, etc. with actual or potential use or value (sensu lato) for
					humanity. Fishery resources are those aquatic resources of
					value to fisheries".</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:AqResIdent" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:History" />
					<xs:element ref="fi:HabitatBio" />
					<xs:element ref="fi:GeoDist" />
					<xs:element ref="fi:WaterAreaOverview" />
					<xs:element ref="fi:AqResStruct" />
					<xs:element ref="fi:Exploit" />
					<xs:element ref="fi:Statistics" />
					<xs:element ref="fi:BioAssess" />
					<xs:element ref="fi:Management" />
					<xs:element ref="fi:AqResStateTrend" />
					<xs:element ref="fi:RelatedObs" />
					<xs:group ref="fi:indexEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AqResIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic resource identity</Title>
				<Description>Topic element used to specify the identity of the aquatic resource. An aquatic resource is identified
					by a title, an association of
					references to Species and Water Areas, or a unique FIGIS system identifier.
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="dc:Title" />
					<xs:sequence>
						<xs:element ref="fi:SpeciesList" maxOccurs="unbounded" />
						<xs:element ref="fi:WaterAreaList" />
						<xs:element ref="fi:ReferenceYear" />
					</xs:sequence>
				</xs:choice>
				<xs:element ref="fi:ReportingYear" minOccurs="0" />
				<xs:element ref="fi:ForeignID" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="fi:AdditionalRefData" minOccurs="0" />
				<xs:element ref="fi:Monitoring" minOccurs="0" />
			</xs:sequence>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute name="Rank" type="xs:positiveInteger" />
			<xs:attribute name="Factsheet" type="xs:boolean" />
			<xs:attribute name="RefObservation" type="xs:boolean" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AqResOverview">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Resource overview</Title>
				<Description>Aquatic Resource overview contains high-level background information on the Aquatic resource instances.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AqResRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AqResRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic resource reference</Title>
				<Description> Element used to Refer to an aquatic resource. It contains species and water area domain elements which
					together enable positive
					referencing of an aquatic resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="dc:Title" />
					<xs:element ref="fi:ForeignID" />
				</xs:choice>
				<xs:element ref="dcterms:Alternative" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="fi:SpeciesRef" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="fi:WaterAreaRef" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="fi:CollectionRef" minOccurs="0" />
				<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AqResStateTrend">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Resource State and Trends</Title>
				<Description>AqResStateTrend is a container for Aquatic Resource State and Trend information entries. "This element
					includes a short statement
					(which should be understandable by the public) summarising the current observation made
					on the resource or stock considered, with a focus on the
					state of the fish population and the trends characterising
					its evolution, and possibly outlooks on its future. For resources units defined at
					aggregated level, this topic may
					consist of a general statement including a list of state and trends entries fitting each of the sub-units making
					up
					that aggregated resource" (FIGIS-FIRMS 2002 workshop).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:sequence minOccurs="0">
					<xs:element ref="fi:ExploitRate" />
					<xs:element ref="fi:AbundanceLevel" />
				</xs:sequence>
				<xs:element ref="fi:ExploitState" minOccurs="0" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="dc:Title" />
					<xs:element ref="fi:AqResStateTrendEntry" />
					<xs:group ref="fi:textEnt" />
					<xs:element ref="fi:History" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AqResStateTrendEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Resource State and Trend information entry</Title>
				<Description>Use an fi:AqResStateTrendEntry for each instance of a state or trend statement, in case distinct
					statements applying to different
					aquatic resources under review are included.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:sequence minOccurs="0">
					<xs:element ref="fi:ExploitRate" />
					<xs:element ref="fi:AbundanceLevel" />
				</xs:sequence>
				<xs:element ref="fi:ExploitState" minOccurs="0" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="dc:Title" />
					<xs:group ref="fi:textEnt" />
					<xs:element ref="fi:AqResRef" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AqResStruct">
		<xs:annotation>
			<xs:documentation>
				<Title>Resource structure</Title>
				<Description>Information about structure of the aquatic resource. This element includes an attribute describing
					whether Yes or Not the aquatic
					resource is considered a biological Stock. It might as well introduce one or more
					subunit(s) of the aquatic resource under consideration (use
					fi:AqStockRef to positively refer a lower level of
					aggregation of an aquatic resource). Where possible, a short explanation should be provided to
					justify the existence
					of the sub-unit(s).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AqResRef" />
				<xs:element ref="fi:History" />
			</xs:choice>
			<xs:attribute name="BiologicalStock" type="xs:boolean" use="required" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AquacultureProduction">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture Production</Title>
				<Description>contains information about aquaculture production.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:FisheryIndicator" />
				<xs:element ref="fi:FisheryIndicatorList" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AquacultureProductionSector">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture Production Sector</Title>
				<Description>contains information on a Sector's aquaculture sector. Select the environment Type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AquacultureProduction" />
				<xs:element ref="fi:CultureSystems" />
				<xs:element ref="fi:FarmingSystems" />
				<xs:element ref="fi:ProductionSites" />
				<xs:element ref="fi:CulturedSpecies" />
				<xs:element ref="fi:Credit" />
				<xs:element ref="fi:Interactions" />
				<xs:element ref="fi:Employment" />
				<xs:element ref="fi:Stakeholders" />
				<xs:element ref="fi:Community" />
				<xs:element ref="fi:NASORef" />
				<xs:element ref="fi:FACPRef" />
			</xs:choice>
			<xs:attributeGroup ref="fi:environEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AquacultureSector">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture SubSector</Title>
				<Description>contains information on a Sector's aquaculture sector. Select the environment Type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AquacultureProduction" />
				<xs:element ref="fi:CultureSystems" />
				<xs:element ref="fi:FarmingSystems" />
				<xs:element ref="fi:ProductionSites" />
				<xs:element ref="fi:CulturedSpecies" />
				<xs:element ref="fi:Credit" />
				<xs:element ref="fi:Interactions" />
				<xs:element ref="fi:Employment" />
				<xs:element ref="fi:Stakeholders" />
				<xs:element ref="fi:Community" />
			</xs:choice>
			<xs:attributeGroup ref="fi:environEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AquacultureUse">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture Use</Title>
				<Description>Element contains information on weather the speceis is used in Aquaculture and if this is the case
					which is the most use way of
					reproduce it.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Widely_used" />
						<xs:enumeration value="Rarely_used" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ReproductionMode">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Assisted_artificial" />
						<xs:enumeration value="Natural" />
						<xs:enumeration value="Continuous_imports" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AreaFeature">
		<xs:annotation>
			<xs:documentation>
				<Title>Area Feature</Title>
				<Description>AreaFeature can contain any domain which has a geo-political aspect to its definition.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AreaIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>figis Area Identity</Title>
				<Description>AreaIdent contains all the possible codes used to positively identify an area object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="6" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:choice>
						<xs:element ref="fi:WaterAreaRef" />
						<xs:element ref="fi:LandAreaRef" />
					</xs:choice>
				</xs:choice>
				<xs:element ref="fi:Image" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Water" />
						<xs:enumeration value="Land" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AreaProfile">
		<xs:annotation>
			<xs:documentation>
				<Title>Area Profile</Title>
				<Description>AreaProfile contains the elements used by domain specialists to differentiate objects from one another.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AreaStruct" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AreaStruct">
		<xs:annotation>
			<xs:documentation>
				<Title>Area Structure</Title>
				<Description>AreaStruct contains sub areas of the fi:Area container in which it's found. Add another fi:Area for
					every subarea and they will be
					created as new objects in the FIGIS database.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:WaterAreaRef" />
				<xs:element ref="fi:LandAreaRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ArrangementEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Arrangemententry</Title>
				<Description>contains information about an instance of an arrangement.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:LegalRelation" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:SpatialScale" />
			</xs:choice>
			<xs:attribute name="Role" type="xs:string" />
			<xs:attributeGroup ref="fi:legalStatusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Arrangements">
		<xs:annotation>
			<xs:documentation>
				<Title>Arrangements</Title>
				<Description>contains information about legal arrangements.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ArrangementEntry" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:SpatialScale" />
			</xs:choice>
			<xs:attribute name="Role" type="xs:string" />
			<xs:attributeGroup ref="fi:legalStatusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessData">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment data</Title>
				<Description>Data contains data, or information about data, used in the context of this assessment method to
					generate the exploitation indicators,
					or presented to the scientific assessment for the calculation of assessment
					indicators.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="fi:textEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Fishery Catch and Effort" />
						<xs:enumeration value="Vessel Surveys" />
						<xs:enumeration value="Tagging" />
						<xs:enumeration value="Remote Sensing" />
						<xs:enumeration value="Environmental Data" />
						<xs:enumeration value="Others" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessEmpiricalApproach">
		<xs:annotation>
			<xs:documentation>
				<Title>Empirical Assessments</Title>
				<Description>Assessments based on an estimation or observation (e.g. catch only, catch trends, delphi approaches)
					and not based on analytical
					assessment models.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AssessEmpiricalApproachEntry" />
			</xs:choice>
			<xs:attribute name="Value" type="xs:string" use="optional" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessEmpiricalApproachEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Empirical Assessment entry</Title>
				<Description>Use fi:AssessEmpiricalApproachEntry for each empirical approach used in performing a resource
					assessment, e.g. “catch and cpue
					trends”, “catch only”, “Delphi method”, etc.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:group ref="fi:textEntMandTitle" />
				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:Results" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessIndicator">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment indicator</Title>
				<Description>Assessment indicator contains indicator data commonly generated using stock assessment methods (e.g.
					fishing mortality, biomass,
					recruitment, abundance index). According to the FAO Fisheries Glossary, an indicator is
					"a variable, pointer, or index. Its fluctuation reveals
					the variations in key elements of a system. The position and
					trend of the indicator in relation to reference points or values indicate the
					present state and dynamics of the
					system. Indicators provide a bridge between objectives and action".</Description>
			</xs:documentation>
			<xs:appinfo>
				<restrict>
					<restrictDomain>AqRes</restrictDomain>
				</restrict>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="fi:textEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Biomass" />
						<xs:enumeration value="Abundance" />
						<xs:enumeration value="Exploitation rate" />
						<xs:enumeration value="SSB" />
						<xs:enumeration value="Recruitment" />
						<xs:enumeration value="Fishing mortality" />
						<xs:enumeration value="Average Weight" />
						<xs:enumeration value="Average length" />
						<xs:enumeration value="Others" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessmentSummary">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment summary</Title>
				<Description>xxxxx</Description>
			</xs:documentation>
			<xs:appinfo>
				<restrict>
					<restrictDomain>Fishery</restrictDomain>
				</restrict>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:RelatedAquaticResources" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessModelEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment model entry</Title>
				<Description>Use fi:AssessModelEntry for each model used in performing a stock assessment. "An assessment model is
					identified by its title, and
					ideally includes the description of available data, assumptions made and methodology
					applied, and a summary of results".</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:group ref="fi:textEntMandTitle" />
				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:Assumption" />
					<xs:element ref="fi:Methodology" />
					<xs:element ref="fi:Data" />
					<xs:element ref="fi:Results" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Type" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AssessModels">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment models</Title>
				<Description>Assessment models describes the various models used in performing a stock assessment. It might contain
					a reference list of models
					commonly used in stock assessments.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Assumption" />
				<xs:element ref="fi:Methodology" />
				<xs:element ref="fi:AssessModelEntry" />
			</xs:choice>
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Age-structured" />
						<xs:enumeration value="Size-structured" />
						<xs:enumeration value="Biomass-aggregated" />
						<xs:enumeration value="Others" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Assumption">
		<xs:annotation>
			<xs:documentation>
				<Title>Assumption</Title>
				<Description>Assumption is a description of the underlying assumptions upon which stock assessments, or results are
					based.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="AuthorisationSystem">
		<xs:annotation>
			<xs:documentation>
				<Title>AuthorisationSystem</Title>
				<Description>contains information about legal authorisation systems for aquaculture and/or fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:License" />
				<xs:element ref="fi:Concession" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:TermsAndConditions" />
				<xs:element ref="fi:Procedures" />
				<xs:element ref="fi:SiteIdentification" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Avg">
		<xs:annotation>
			<xs:documentation>
				<Title>Average</Title>
				<Description>contains a numeric average value.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Background">
		<xs:annotation>
			<xs:documentation>
				<Title>Background</Title>
				<Description>contains Background information on Fisheries Topics.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BackgroundRequirements">
		<xs:annotation>
			<xs:documentation>
				<Title>Background Requirements - Formal Knowledge/Data</Title>
				<Description>Definition for EAF Tool domain: What level of formal background knowledge, datasets or preparatory work
					must be available and
					completed to effectively use the EAF tool? Some tools, in addition to the capacity needed to
					use it, also require considerable levels of
					background information and extensive data sets before they can be
					usefully implemented even if the technical capacity is available (e.g. stock
					assessment models; ecosystem models).
					Others need only the information from individual’s experiences.
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:Range" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Bathymetry">
		<xs:annotation>
			<xs:documentation>
				<Title>Bathymetry</Title>
				<Description>Bathymetry is numeric depth information about the ranges in which an aquatic species is found.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Min" />
				<xs:element ref="fi:Max" />
				<xs:element ref="fi:DiscreteValue" />
			</xs:choice>
			<xs:attributeGroup ref="fi:lengthUnit" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Beam">
		<xs:annotation>
			<xs:documentation>
				<Title>Beam</Title>
				<Description>is the width of a vessel at its widest point.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:lengthUnit" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BiblioEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Bibliographic entry</Title>
				<Description>BiblioEntry is a container for elements that define a reference source. It is similar in spirit to the
					bibliographies found in
					traditional books. All the fields that would be expected in a Bibliographic Entry are
					included, as well as the possibility of tagging hyperlinks
					and adding free text.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:Series" />
				<xs:element ref="ags:CreatorPersonal" />
				<xs:element ref="ags:CreatorCorporate" />
				<xs:element ref="fi:Editor" />
				<xs:element ref="dcterms:Created" />
				<xs:element ref="dcterms:Modified" />
				<xs:element ref="dc:Identifier" />
				<xs:element ref="dc:Description" />
				<xs:element ref="dcterms:Abstract" />
				<xs:element ref="dcterms:bibliographicCitation" />
				<xs:element ref="dc:Subject" />
				<xs:element ref="dc:Publisher" />
				<xs:element ref="dc:Language" />
				<xs:element ref="dcterms:Extent" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Bibliography">
		<xs:annotation>
			<xs:documentation>
				<Title>Bibliography</Title>
				<Description>Bibliography is a container for biblio entry elements. It is used as in a traditional print
					bibliography found at the end of a
					document, in order to cite the literature consulted and used for the creation of
					the document.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:BiblioEntry" />
			</xs:choice>
			<xs:attribute name="RefersTo">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Block" />
						<xs:enumeration value="Entire_file" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BioAssess">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment</Title>
				<Description>Assessment is a container for elements describing the biological assessment of the aquatic resource,
					such as the "description of data
					sources used for the assessment, the various assessment models and related methods
					applied with their individual results, and the overall
					assessment results, including possibly scientific advice"
					(FIGIS-FIRMS 2002 workshop).
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:AssessModels" />
				<xs:element ref="fi:AssessEmpiricalApproach" />
				<xs:element ref="fi:Data" />
				<xs:element ref="fi:Results" />
				<xs:element ref="fi:SciAdvice" />
				<xs:element ref="fi:History" />
				<xs:element ref="fi:Uncertainty" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BiologicalControl">
		<xs:complexType mixed="false">
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="SnailControl" />
						<xs:enumeration value="WeedControl" />
						<xs:enumeration value="OtherPestControl" />
						<xs:enumeration value="MosquitoControl" />
						<xs:enumeration value="PlanktonControl" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BiologicalParameters">
		<xs:annotation>
			<xs:documentation>
				<Title>Biological Parameters</Title>
				<Description>Biological Parameters is a container for information directly related to the parameters used to
					describe the biology of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="fi:textEnt" />
				</xs:choice>
				<xs:element ref="fi:Length" minOccurs="0" />
				<xs:element ref="fi:Weight" minOccurs="0" />
				<xs:element ref="fi:Age" minOccurs="0" />
				<xs:element ref="fi:Fecundity" minOccurs="0" />
				<xs:element ref="fi:Mortality" minOccurs="0" />
				<xs:element ref="fi:BiologicalRatio" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BiologicalRatio">
		<xs:annotation>
			<xs:documentation>
				<Title>Biological Ratio</Title>
				<Description>Biological Ratio is a container for information directly related to the Biological Ratios used to
					describe a species or population.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Sex" />
						<xs:enumeration value="Length-weight" />
						<xs:enumeration value="Length-length" />
						<xs:enumeration value="Other_type" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Unit" type="xs:string" />
			<xs:attributeGroup ref="fi:speciesAtt" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BottomType">
		<xs:annotation>
			<xs:documentation>
				<Title>Bottom environment</Title>
				<Description>"Marine habitat: type of bottom substratum in which the fishery resource is distributed." (FIRMS
					Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Seagrass" />
						<xs:enumeration value="Mangrove" />
						<xs:enumeration value="Coral_reef" />
						<xs:enumeration value="Soft_bottom" />
						<xs:enumeration value="Soft_bottom_with_pebbles" />
						<xs:enumeration value="Soft_bottom_with_gravel" />
						<xs:enumeration value="Soft_bottom_gravel-sandy" />
						<xs:enumeration value="Soft_bottom_clean_sand" />
						<xs:enumeration value="Soft_bottom_muddy_or_muddy-sand" />
						<xs:enumeration value="Soft_bottom_with_some_turbidity" />
						<xs:enumeration value="Hard_bottom" />
						<xs:enumeration value="Hard_rocky_bottom" />
						<xs:enumeration value="Hard_bottom_with_clear_water" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FreeValue" use="optional" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Broodstock">
		<xs:annotation>
			<xs:documentation>
				<Title />
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Collection" />
						<xs:enumeration value="production" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="BudgetCode">
		<xs:annotation>
			<xs:documentation>
				<Title>Budget Code</Title>
				<Description>contains the budget code for a FAO budget item.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="BuildMaterial">
		<xs:annotation>
			<xs:documentation>
				<Title>Build Material</Title>
				<Description>contains information about the kind of material used in building a vessel.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Metal" />
						<xs:enumeration value="Wood" />
						<xs:enumeration value="Fiberglass" />
						<xs:enumeration value="Other" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Built">
		<xs:annotation>
			<xs:documentation>
				<Title>Built</Title>
				<Description>contains elements related to the time and place that a ship was built.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="dc:Date" />
				<xs:element ref="fi:Shipyard" />
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:LandAreaRef" />
				<xs:element ref="fi:Nation" />
				<xs:element ref="fi:City" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Bycatch">
		<xs:annotation>
			<xs:documentation>
				<Title>By-catch</Title>
				<Description>"Part of a catch of a fishing unit taken incidentally in addition to the target species towards which
					fishing effort is directed.
					Some or all of it may be returned to the sea as discards, usually dead or dying.
					Associated species: commercial species caught with the main
					target ones. Discarded species: those species, including
					for specific life stages, released or returned to the sea, dead or alive, whether or not
					such organisms are brought
					fully on board a fishing vessel. Protected species: species (marine mammals, seabirds etc.) caught accidentally
					with
					the main target ones." (Fisheries Inventory: Method and Guidelines. FIRMS, 2007).</Description>
				<Usage>is a container for listing all the bycatch species caught by a fishery. Use the Type attribute to
					differentiate such bycatch.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:SpeciesList" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="AssociatedSpecies" />
						<xs:enumeration value="DiscardedSpecies" />
						<xs:enumeration value="ProtectedSpecies" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CanBeConf">
		<xs:annotation>
			<xs:documentation>
				<Title>Can be Confused</Title>
				<Description>CanBeConf is a description of why a species may be confused with another. It may instead/also contain a
					list of other species with
					which a species may be confused.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Capacity">
		<xs:annotation>
			<xs:documentation>
				<Title>Capacity</Title>
				<Description>is a number representing a system's maximum sustainable output.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Unit" type="xs:string" />
			<xs:attribute name="Value" type="xs:decimal" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CapturedSpecies">
		<xs:annotation>
			<xs:documentation>
				<Title>Captured Species</Title>
				<Description>All the species caught by a fishery.</Description>
				<Usage>Those species considered part of the catch, according to the criteria used in the concerned fishery. These
					criteria would usually refer to
					the on board retained catch, including possible protected species. Use Target
					species, Associated and By-catch species to differentiate such
					catches.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:SpeciesList" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CaptureProduction">
		<xs:annotation>
			<xs:documentation>
				<Title>Capture Production</Title>
				<Description>Catch and landing data by main species groups, fisheries and/or areas.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:FisheryIndicator" />
				<xs:element ref="fi:FisheryIndicatorList" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CatchHandlingProcEquip">
		<xs:annotation>
			<xs:documentation>
				<Title>Catch Handling and Processing Equipment</Title>
				<Description>is a container to describe the way catches are handled on board and which are the on-board processing
					facilities (freezer, etc).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ChapterTitle">
		<xs:annotation>
			<xs:documentation>
				<Title>Chapter Title</Title>
				<Description>ChapterTitle contains the name of a chapter.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Chart">
		<xs:annotation>
			<xs:documentation>
				<Title>Chart</Title>
				<Description>contains a database link to a chart.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" minOccurs="0" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="DKey" type="xs:string" use="required" />
			<xs:attribute name="DKey2" use="optional" />
			<xs:attribute name="Value" type="xs:string" use="required" />
			<xs:attribute name="Value2" use="optional" />
			<xs:attribute name="Ds" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="City">
		<xs:annotation>
			<xs:documentation>
				<Title>City</Title>
				<Description>City contains the name of a city used in the formation of the postal address for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Class">
		<xs:annotation>
			<xs:documentation>
				<Title>Class</Title>
				<Description>Class is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Taxonomic">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Taxonomic" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:asfis" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ClassificationSystem">
		<xs:annotation>
			<xs:documentation>
				<Title>RelatedStandards</Title>
				<Description>contains the description of a classification system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:StandardRef" />
				<xs:element ref="fi:Instance" />
				<xs:element ref="fi:EncodingScheme" />
				<xs:element ref="fi:Rule" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ClimaticZone">
		<xs:annotation>
			<xs:documentation>
				<Title>Climate environment</Title>
				<Description>"Marine habitat: type of climate prevailing in the area of distribution of the fishery resource."
					(FIRMS Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Polar" />
						<xs:enumeration value="Temperate" />
						<xs:enumeration value="Tropical" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CodeConduct">
		<xs:annotation>
			<xs:documentation>
				<Title>CodeConduct</Title>
				<Description>contains information about codes of conduct and/or guidelines.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Guidelines" />
				<xs:element ref="fi:SelfMonitoring" />
				<xs:element ref="fi:ProductCertification" />
				<xs:element ref="fi:OrgsInvolved" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CodeSystem">
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Collection">
		<xs:complexType mixed="false">
			<xs:choice>
				<xs:sequence>
					<xs:element ref="fi:CollectionRef" />
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="fi:CollectionProfile" />
						<xs:element ref="fi:CollectionFeature" />
					</xs:choice>
				</xs:sequence>
				<xs:sequence>
					<xs:element ref="fi:CollectionIdent" />
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element ref="fi:CollectionProfile" />
						<xs:element ref="fi:CollectionFeature" />
					</xs:choice>
					<xs:group ref="fi:indexEnt" minOccurs="0" />
				</xs:sequence>
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionCoverage">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Key" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionFeature">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:StateOfActivity" />
				<xs:element ref="ags:RightsTermsOfUse" />
				<xs:element ref="fi:DataSource" />
				<xs:element ref="fi:Disclaimer" />
				<xs:element ref="fi:QualityAssurance" />
				<xs:element ref="fi:TypicalUsage" />
				<xs:element ref="dcterms:Audience" />
				<xs:element ref="dc:Format" />
				<xs:element ref="fi:Processing" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionIdent">
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="6" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:sequence>
						<xs:element ref="dc:Identifier" />
						<xs:element ref="dc:Date" />
						<xs:element ref="fi:BudgetCode" minOccurs="0" />
						<xs:element ref="fi:OrgRef" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionMethod">
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionOverview">
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Subject" />
				<xs:element ref="dc:Description" />
				<xs:element ref="dc:Coverage" />
				<xs:element ref="fi:Glossary" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionProfile">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:CollectionOverview" />
				<xs:element ref="fi:DatasetOverview" />
				<xs:element ref="fi:ItemsMeasured" />
				<xs:element ref="fi:CollectionStruct" />
				<xs:element ref="fi:CollectionCoverage" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Collection reference</Title>
				<Description>contains the elements necessary for positive referencing of a data collection.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:ForeignID" />
				</xs:choice>
				<xs:element ref="fi:OrgRef" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CollectionStruct">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ItemsMeasured" />
				<xs:element ref="fi:Subset" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Community">
		<xs:annotation>
			<xs:documentation>
				<Title>Community</Title>
				<Description>is the container for identifying people's communities (fishermen communities, ethnic groups, ...) also
					intended in terms of industry
					stakeholder, fishery folks, retailers, etc. The attribute Participation is utilized
					for indicating which level of community or participation is
					possible or required.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Participation" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Concession">
		<xs:annotation>
			<xs:documentation>
				<Title>Concession</Title>
				<Description>contains information about legal concessions to engage in aquaculture and/or fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ConservStatus">
		<xs:annotation>
			<xs:documentation>
				<Title>Conservation Status</Title>
				<Description>ConservStatus is a review of the conservation status of the species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:code" />
			<xs:attribute name="CodeSystem">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="IUCN" />
						<xs:enumeration value="CITES" />
						<xs:enumeration value="Other" />
						<xs:enumeration value="None" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ConsolidatedDate">
		<xs:annotation>
			<xs:documentation>
				<Title>Consolidated Date</Title>
				<Description>ConsolidatedDate contains the last consolidated date of a legal object. "The date in which a provision
					has been consolidated
					including amending texts, should be together with 'date of original text' " (LEGN, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Constraints">
		<xs:annotation>
			<xs:documentation>
				<Title>Constraints</Title>
				<Description>contains text regarding constraints on development prospects and/or projects. Use the fi:FIGISLink tags
					to indicate any keywords
					useful in determining the keys related to the constraint information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Contact</Title>
				<Description>Contact is a generic container for elements containing contact information for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:PostalAddress" />
				<xs:element ref="fi:Telephone" />
				<xs:element ref="fi:Fax" />
				<xs:element ref="fi:Telex" />
				<xs:element ref="fi:Email" />
				<xs:element ref="fi:Website" />
				<xs:group ref="fi:textEntNoTitle" minOccurs="0" maxOccurs="unbounded" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Contacts">
		<xs:annotation>
			<xs:documentation>
				<Title>Contacts</Title>
				<Description>Contacts contains paper and electronic contact information for MAILING fi:List , departments ,
					programmes , projects and experts.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:OrgRef" />
				<xs:element ref="fi:ContactEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ControlMeasures">
		<xs:annotation>
			<xs:documentation>
				<Title>Control Measures</Title>
				<Description>is a container for control measures for diseases affecting aquatic species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ControlSystemEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>ControlSystementry</Title>
				<Description>contains information about one aspect of a disease control system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Surveillance" />
						<xs:enumeration value="Zoning" />
						<xs:enumeration value="Early_warning" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ControlSystems">
		<xs:annotation>
			<xs:documentation>
				<Title>ControlSystems</Title>
				<Description>contains information about disease control systems.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ControlSystemEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Cooperation">
		<xs:annotation>
			<xs:documentation>
				<Title>Cooperation</Title>
				<Description>is a container element for overviews of cooperative frameworks. Use instances of CooperationEntry to
					profile various frameworks.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:CooperationEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CooperationEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Cooperation entry</Title>
				<Description>is a container element for individual instances of cooperative frameworks.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:OrgsInvolved" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CorporateCoverPage">
		<xs:annotation>
			<xs:documentation>
				<Title>Corporate cover page</Title>
				<Description>Corporate Cover Page is a container for bibliographic elements commonly found on the cover or inset of
					a print publication,
					particularly title , corporate author , series, publisher. It is a mandatory element of
					fi:ObjectSource and contributes to build the citation of
					a domain object presented as FIGIS fact sheet or web site.
					The two required elements are: dc:Title - contributes part or entirety of the title of
					the web page, and the
					ags:CreatorCorporate tag.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:ForeignID" />
				</xs:choice>
				<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6" />
				<xs:element ref="ags:CreatorCorporate" minOccurs="0" maxOccurs="6" />
				<xs:element ref="fi:Series" minOccurs="0" maxOccurs="6" />
				<xs:element ref="dc:Identifier" minOccurs="0" />
				<xs:element ref="dc:Subject" minOccurs="0" />
				<xs:element ref="dc:Publisher" minOccurs="0" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CostEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Cost entry</Title>
				<Description>are the costs associated with an object. Indicate the usable lifetime if desired.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Value">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Low" />
						<xs:enumeration value="Moderate" />
						<xs:enumeration value="High" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="UsableLifetime" type="xs:string" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Costs">
		<xs:annotation>
			<xs:documentation>
				<Title>Costs</Title>
				<Description>is a container for instances of fi:Cost.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:CostEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountryFrom">
		<xs:annotation>
			<xs:documentation>
				<Title>Country from</Title>
				<Description>Refers to the country of origin of a species that has been introduced.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:LandAreaRef" />
				<xs:choice minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountrySector">
		<xs:annotation>
			<xs:documentation>
				<Title>Country Sector</Title>
				<Description>This is the root element for fisheries country profiles domain. A country's entire fisheries sector can
					be profiled or any one of its
					subsectors, marine, aquaculture, inland, recreational and processing.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:CountrySectorIdent" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:CountrySectorProfile" />
					<xs:element ref="fi:CountrySectorFeature" />
					<xs:element ref="fi:Overview" />
					<xs:element ref="fi:History" />
					<xs:element ref="fi:GenGeoEconReport" />
					<xs:element ref="fi:Statistics" />
					<xs:element ref="fi:ProductionSector" />
					<xs:element ref="fi:PostHarvest" />
					<xs:element ref="fi:SectorSocioEcoContribution" />
					<xs:element ref="fi:StatusTrends" />
					<xs:element ref="fi:OrgsInvolved" />
					<xs:element ref="fi:LegalFrameworkOverview" />
					<xs:element ref="fi:AddInfo" />
					<xs:element ref="fi:EIMS" />
					<xs:element ref="fi:NEMS" />
					<xs:element ref="fi:FAOLex" />
				</xs:choice>
				<xs:group ref="fi:indexEnt" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Variation" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountrySectorFeature">
		<xs:annotation>
			<xs:documentation>
				<Title>Country Sector Features</Title>
				<Description>Feature is a container for supplementary elements related to the object but not essential to its
					Identification or distinction, e.g.
					for an aquatic species this would be elements such as conservation status or
					interest to fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:CountrySectorIndustryProfile" />
				<xs:element ref="fi:PostHarvest" />
				<xs:element ref="fi:Management" />
				<xs:element ref="fi:Markets" />
				<xs:element ref="fi:Performance" />
				<xs:element ref="fi:StatusTrends" />
				<xs:element ref="fi:Development" />
				<xs:element ref="fi:OrgOverview" />
				<xs:element ref="fi:LegalFrameworks" />
				<xs:element ref="fi:Stakeholders" />
				<xs:element ref="fi:CountrySectorManage" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountrySectorIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Country Sector Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify an object. Inclusion of this
					element in a document causes the
					FIGIS database loader to create a database object that will be keyed using the
					information found in the Ident element. Identify the sector by
					choosing one of the type values. Select the
					appropriate document status</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:LandAreaRef" />
				</xs:choice>
				<xs:element ref="fi:ReportingYear" />
				<xs:element ref="fi:Image" minOccurs="0" />
			</xs:sequence>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountrySectorIndustryProfile">
		<xs:annotation>
			<xs:documentation>
				<Title>Country Sector Industry Profile</Title>
				<Description>is a container for industry subsector information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Overview" />
				<xs:element ref="fi:AquacultureSector" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountrySectorManage">
		<xs:annotation>
			<xs:documentation>
				<Title>Country Sector Management</Title>
				<Description>is a container element for overviews of a Sector's fisheries management. Use instances of
					fi:ManageSystemOverview to profile various
					management systems, including overview of stocks exploited or fisheries
					managed within the system and provide information if one or several
					stocks or fishing capacity/effort are subject to
					management decision-making with a bilateral or regional fishery management arrangement
					(reference should be made to
					the respective bilateral or regional fishery management arrangement.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:Overview" />
				<xs:element ref="fi:Cooperation" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:LegalFrameworks" />
			</xs:choice>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountrySectorProfile">
		<xs:annotation>
			<xs:documentation>
				<Title>Country Sector Profile</Title>
				<Description>Profile contains the elements used by domain specialists to profile an object and declare it as a
					unique type. That is, rather than
					containing the Identifying codes or names (Keys) for the object as in the Ident
					element, the Profile block contains a description of the central
					components of an object, the information that is
					intrinsic and/or invariable to the object, e.g. for an aquatic species this would be elements
					such as fieldmarks or
					diagnostic features.
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:Overview" />
				<xs:element ref="fi:History" />
				<xs:element ref="fi:GenGeoReport" />
				<xs:element ref="fi:GenEconReport" />
				<xs:element ref="fi:Statistics" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CountryTo">
		<xs:annotation>
			<xs:documentation>
				<Title>Country To</Title>
				<Description>Refers to the country where a given species has been introduced.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:LandAreaRef" />
				<xs:choice minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CoverPage">
		<xs:annotation>
			<xs:documentation>
				<Title>Cover page</Title>
				<Description>Cover Page is a container for bibliographic elements commonly found on the cover or inset of a print
					publication, and particularly
					personal author(s), publication date , and abstract or description. It is an optional
					element of fi:ObjectSource and contributes to build the
					citation of a domain object presented as FIGIS fact sheet or
					web. If there are multiple creators use the ags:CreatorPersonal. dc:Created - this
					is the date of the creation of
					the intellectual content of the information resource. Recommended best practice for encoding the date value is
					defined in a profile of ISO 8601 [W3CDTF] and follows the YYYY-MM-DD format.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="ags:CreatorPersonal" />
				<xs:element ref="dcterms:Created" />
				<xs:element ref="dcterms:Modified" />
				<xs:element ref="dc:Language" />
				<xs:element ref="dc:Description" />
				<xs:element ref="dc:Subject" />
				<xs:element ref="dcterms:Abstract" />
				<xs:element ref="dcterms:Alternative" />
				<xs:element ref="dc:Title" />
				<xs:element ref="dcterms:replaces" />
				<xs:element ref="dcterms:isReplacedBy" />
				<xs:element ref="fi:ChapterTitle" />
				<xs:element ref="fi:VolumeTitle" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Credit">
		<xs:annotation>
			<xs:documentation>
				<Title>Credit</Title>
				<Description>contains information about development credit.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Crew">
		<xs:annotation>
			<xs:documentation>
				<Title>Crew</Title>
				<Description>contains information about the number and ethnicity of a vesseltype crew.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="CrewNumber" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CulturedSpecies">
		<xs:annotation>
			<xs:documentation>
				<Title>Cultured Species</Title>
				<Description>contains a list of cultured species and/or species groups. Use fi:CulturedSpeciesEntry for each species
					or species group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:CulturedSpeciesEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CulturedSpeciesEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Cultured Species entry</Title>
				<Description>contains a cultured species and/or species group. Tag the species names with fi:FIGISLink tags. If
					desired, indicate the Type and
					History of the species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:HabitatBio" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Indigenous" />
						<xs:enumeration value="Introduced" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="History">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Continuing" />
						<xs:enumeration value="New" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CultureSpecies">
		<xs:annotation>
			<xs:documentation>
				<Title>Cultured Species</Title>
				<Description>CultureSpecies is the Cultured Species domain root element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:CultureSpeciesIdent" />
				<xs:element ref="fi:CultureSpeciesProfile" minOccurs="0" />
				<xs:element ref="fi:CultureSpeciesFeature" minOccurs="0" />
				<xs:group ref="fi:indexEnt" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CultureSpeciesFeature">
		<xs:annotation>
			<xs:documentation>
				<Title>Cultured Species Feature</Title>
				<Description>CultureSpeciesFeature is a container for supplementary descriptive elements related to a cultured
					species but not essential to its
					identification or distinction.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ProductionSystems" />
				<xs:element ref="fi:Diseases" />
				<xs:element ref="fi:AquacultureProduction" />
				<xs:element ref="fi:Markets" />
				<xs:element ref="fi:StatusTrends" />
				<xs:element ref="fi:Issues" />
				<xs:element ref="fi:ResponsiblePractices" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CultureSpeciesIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Cultured Species</Title>
				<Description>CultureSpeciesIdent contains the key identifiers for the domain.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" />
				<xs:sequence>
					<xs:element ref="fi:SpeciesRef" />
					<xs:element ref="fi:Image" />
				</xs:sequence>
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Status" type="xs:integer" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CultureSpeciesProfile">
		<xs:annotation>
			<xs:documentation>
				<Title>Cultured Species Profile</Title>
				<Description>CultureSpeciesProfile contains the elements used by domain specialists to profile a cultured species
					and declare it as a unique type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:History" />
				<xs:element ref="fi:GeoDist" />
				<xs:element ref="fi:HabitatBio" />
				<xs:element ref="fi:Size" />
				<xs:element ref="fi:ProductionCycle" />
				<xs:element ref="fi:LifeCycle" />
				<xs:element ref="fi:Images" />
				<xs:element ref="fi:DiagnosticFeat" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CultureSystemEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Culture System</Title>
				<Description>contains information about a single type of aquaculture production. Include the Type and Environment if
					desired.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:Capacity" />
				<xs:element ref="fi:Number" />
				<xs:element ref="fi:Volume" />
				<xs:element ref="fi:Value" />
				<xs:element ref="fi:SurfaceArea" />
				<xs:element ref="fi:CulturedSpecies" />
				<xs:element ref="fi:ProductionSites" />
			</xs:choice>
			<xs:attributeGroup ref="fi:byType" />
			<xs:attributeGroup ref="fi:environEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="CultureSystems">
		<xs:annotation>
			<xs:documentation>
				<Title>Culture Systems</Title>
				<Description>contains information about aquaculture production. Use an fi:CultureSystemEntry tag for every distinct
					type of aquaculture
					production.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:CultureSystemEntry" />
				<xs:element ref="fi:ProductionSites" />
				<xs:element ref="fi:PostHarvest" />
				<xs:element ref="fi:SpecialisationIntegration" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Data">
		<xs:annotation>
			<xs:documentation>
				<Title>Data</Title>
				<Description>Data contains "data, or information about data, used to generate the exploitation indicators, or
					presented to the scientific
					assessment for the calculation of assessment indicators" (FIGIS-FIRMS 2002 workshop).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:AssessData" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DataEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Data entry</Title>
				<Description>DataEntry refers to the actual data formatting of a FIGISDoc. This refers to the action of putting the
					document into its current XML
					form. fi:Editor - name of the person who put the document into its current XML form.
					Use a new occurrence of this element for every editor who
					has worked on the XML document. dc:Date - contains the
					date on which the current version was last edited. Use ISO 8601 standard YYYY-MM-DD.
					Version - version number of the
					data in its present form. Add one (1) to the previous number for each cycle in which the document will be
					reloaded
					into the database.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:Editor" />
				<xs:element ref="dcterms:Created" minOccurs="0" />
				<xs:element ref="dcterms:Modified" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="Version" type="xs:decimal" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DatasetOverview">
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Subject" />
				<xs:element ref="dc:Description" />
				<xs:element ref="dc:Coverage" />
				<xs:element ref="fi:Glossary" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DataSource">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:DataSourceEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DataSourceEntry">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:CollectionRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DateObserved">
		<xs:annotation>
			<xs:documentation>
				<Title>Date Observed</Title>
				<Description>is the date of observation.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DateReported">
		<xs:annotation>
			<xs:documentation>
				<Title>Date Reported</Title>
				<Description>is the date of reporting.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DeckArrange">
		<xs:annotation>
			<xs:documentation>
				<Title>Deck Arrangement</Title>
				<Description>DeckArrange is the fishing vessel deck arrangement.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:GeartypeRef" />
				<xs:element ref="fi:VesseltypeRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DeckEquip">
		<xs:annotation>
			<xs:documentation>
				<Title>Deck Equipment</Title>
				<Description>DeckEquip is a vessel's deck equipment.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:GeartypeRef" />
				<xs:element ref="fi:VesseltypeRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DeckType">
		<xs:annotation>
			<xs:documentation>
				<Title>Decktype</Title>
				<Description>DeckType is a parameter used by the FAO FIES vessel classification system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Decree">
		<xs:annotation>
			<xs:documentation>
				<Title>Decree</Title>
				<Description>Decree contains information about an Decree relating to a legal framework.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:LegalRelation" />
				<xs:element ref="fi:SpatialScale" />
			</xs:choice>
			<xs:attribute name="Role" type="xs:string" />
			<xs:attributeGroup ref="fi:legalStatusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DepthBehav">
		<xs:annotation>
			<xs:documentation>
				<Title>Depth Behaviour</Title>
				<Description>DepthBehav is a container for information on species behaviour related to water depth. 'Bathymetry'
					element is used in the case that
					the depth ranges in which an aquatic species is found are available.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="fi:textEnt" />
				</xs:choice>
				<xs:element ref="fi:Bathymetry" minOccurs="0" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DepthZone">
		<xs:annotation>
			<xs:documentation>
				<Title>Depth Zone</Title>
				<Description>"Marine habitat/Fishing ground: Bathymetric depth range in which a fishery resource is distributed or
					in which a fishery is
					conducted" (FIRMS Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Coastal_0_50m" />
						<xs:enumeration value="Coastal-Intertidal" />
						<xs:enumeration value="Coastal-Close_shore_littoral_20m" />
						<xs:enumeration value="Coastal-Shallow_waters_inshore_50m" />
						<xs:enumeration value="Shelf_50_200m" />
						<xs:enumeration value="Shelf-Uppershelf_up_100m" />
						<xs:enumeration value="Shelf-Deepshelf_100-200m" />
						<xs:enumeration value="Shelf-Edgeshelf" />
						<xs:enumeration value="Slope_200_1000m" />
						<xs:enumeration value="Slope-Upperslope_200-500m" />
						<xs:enumeration value="Slope-Deepslope_500-1000m" />
						<xs:enumeration value="Abyssal_&gt;1000m" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FreeValue" use="optional" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Description">
		<xs:annotation>
			<xs:documentation>
				<Title>Description</Title>
				<Description>contains description information on Fisheries Topics.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Background" />
				<xs:element ref="fi:Development" />
				<xs:element ref="fi:Issues" />
				<xs:element ref="fi:StatusTrends" />
				<xs:element ref="fi:StateOfActivity" />
				<xs:element ref="fi:Action" />
				<xs:element ref="fi:Solutions" />
				<xs:element ref="fi:Outlook" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Development">
		<xs:annotation>
			<xs:documentation>
				<Title>Development</Title>
				<Description>contains fisheries development information. Use containers for more specific information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Strategies" />
				<xs:element ref="fi:Constraints" />
				<xs:element ref="fi:Research" />
				<xs:element ref="fi:Education" />
				<xs:element ref="fi:AidOrInvestments" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:LegalFrameworks" />
			</xs:choice>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DiagnosticFeat">
		<xs:annotation>
			<xs:documentation>
				<Title>Diagnostic Features</Title>
				<Description>DiagnosticFeat contains the diagnostic features of a species , i.e. the anatomical and morphological
					description that permit the
					positive identification of a species. Use a fi:DiagnosticFeatEntry element for every
					specific aspect of diagnostic features.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Fieldmarks" />
				<xs:element ref="fi:CanBeConf" />
				<xs:element ref="fi:DiagnosticFeatEntry" />
				<xs:element ref="fi:Size" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DiagnosticFeatEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Diagnostic Features entry</Title>
				<Description>DiagnosticFeatEntry contains the diagnostic features of a species for a single morphological and
					physiological characteristic.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Body_shape" />
						<xs:enumeration value="Scales" />
						<xs:enumeration value="Fins" />
						<xs:enumeration value="Gills" />
						<xs:enumeration value="Teeth" />
						<xs:enumeration value="Colour" />
						<xs:enumeration value="Skeletal_system" />
						<xs:enumeration value="Physiology" />
						<xs:enumeration value="Morphology" />
						<xs:enumeration value="Other_type" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="OtherType" type="xs:string" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Dimension">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Subject" />
				<xs:element ref="dc:Coverage" />
				<xs:element ref="dc:Type" />
				<xs:element ref="dc:Description" />
				<xs:element ref="fi:Unit" />
				<xs:element ref="fi:Key" />
				<xs:element ref="fi:Dimension" />
				<xs:element ref="fi:AddInfo" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Disclaimer">
		<xs:annotation>
			<xs:documentation>
				<Title>Disclaimer</Title>
				<Description>A disclaimer about the collection data.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="DiscreteValue">
		<xs:annotation>
			<xs:documentation>
				<Title>Discrete Value</Title>
				<Description>contains a discrete value.</Description>
				<Usage>To be used within the Range element.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Text" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DiseaseControl">
		<xs:annotation>
			<xs:documentation>
				<Title>DiseaseControl</Title>
				<Description>contains information about legal aspects of aquaculture disease control.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ControlMeasures" />
				<xs:element ref="fi:ControlSystems" />
				<xs:element ref="fi:Legislation" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DiseaseEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Disease</Title>
				<Description>is a container for a disease affecting aquatic species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:ControlMeasures" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Diseases">
		<xs:annotation>
			<xs:documentation>
				<Title>Diseases</Title>
				<Description>is a container for diseases affecting aquatic species and description of control measures.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:DiseaseEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DistributionList">
		<xs:annotation>
			<xs:documentation>
				<Title>Distribution List</Title>
				<Description>DistributionList is a set of distribution lists for publications.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="6" />
				<xs:element ref="fi:Owner" />
				<xs:element ref="fi:Link" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:OrgRef" />
					<xs:element ref="fi:Sources" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Draught">
		<xs:annotation>
			<xs:documentation>
				<Title>Draught</Title>
				<Description>is the depth of a vessel's keel below the waterline.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:lengthUnit" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="DrugsAndChemicals">
		<xs:annotation>
			<xs:documentation>
				<Title>DrugsAndChemicals</Title>
				<Description>contains information about legal aspects of veterinary Drugs And Chemicals.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Legislation" />
				<xs:element ref="fi:Regulations" />
				<xs:element ref="fi:OrgsInvolved" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFStep">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Step</Title>
				<Description>An EAF Planning and Implementation Tool can belog to a single or multiple steps/activities.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="consultation" />
						<xs:enumeration value="_1_" />
						<xs:enumeration value="_1.1_" />
						<xs:enumeration value="_1.2_" />
						<xs:enumeration value="_1.3_" />
						<xs:enumeration value="_2_" />
						<xs:enumeration value="_2.1_" />
						<xs:enumeration value="_2.2_" />
						<xs:enumeration value="_3_" />
						<xs:enumeration value="_3.1_" />
						<xs:enumeration value="_3.2_" />
						<xs:enumeration value="_3.3_" />
						<xs:enumeration value="_4_" />
						<xs:enumeration value="_4.1_" />
						<xs:enumeration value="_4.2_" />
						<xs:enumeration value="_4.3_" />
						<xs:enumeration value="_4.4_" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFSteps">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Steps</Title>
				<Description>Some EAF tools can assist with a number of steps or activities, some are only relevant to one.</Description>
				<Usage>Use on e EAFStep sub-element for each entry.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:EAFStep" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFTool">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool</Title>
				<Description>The Ecosystem Approach to Fisheries (EAF) includes an EAF Toolbox to assist with EAF management
					planning and implementation. The
					toolbox is made of synoptic descriptions of the available tools, the EAF Tool fact
					sheets.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:EAFToolIdent" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:EAFSteps" />
					<xs:element ref="fi:Objectives" />
					<xs:element ref="fi:Overview" />
					<xs:element ref="fi:EAFToolTips" />
					<xs:element ref="fi:EAFToolPedigree" />
					<xs:element ref="fi:EAFToolSynergy" />
					<xs:element ref="fi:EAFToolUsage" />
					<xs:element ref="fi:EAFToolCapacity" />
					<xs:element ref="fi:BackgroundRequirements" />
					<xs:element ref="fi:Community" />
					<xs:element ref="fi:Costs" />
					<xs:element ref="fi:TimeRange" />
					<xs:element ref="fi:AddInfo" />
					<xs:group ref="fi:indexEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFToolCapacity">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool Capacity</Title>
				<Description>The capacity needed to use a tool depends upon the complexity of the tool. What technical capacity and
					experience is required to use
					it effectively? Some tools require a high degree of technical expertise to operate
					effectively (e.g. simulation modelling), some tools can be
					used with minimal training (e.g. attending a meeting).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="fi:Range" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFToolIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool identity</Title>
				<Description>EAFToolIdent contains identity information about EAF Tool.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:ForeignID" minOccurs="0" maxOccurs="unbounded" />
					<xs:element ref="dc:Title" />
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="fi:statusEnt" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFToolPedigree">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool Pedegree</Title>
				<Description>What is the origin of the tool and where has it already been used successfully?</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFToolSynergy">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool Synergy</Title>
				<Description>Does it have particularly strong synergies with other tools?</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFToolTips">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool Tips</Title>
				<Description>What tricks or tips are there for its use in EAF, including when not to use it?</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EAFToolUsage">
		<xs:annotation>
			<xs:documentation>
				<Title>EAF Tool Usage</Title>
				<Description>How easy or hard is the tool to use? Easy tools should be able to be applied in any fishery system.
					Hard tools will require
					specialised capacity and knowledge and may be unsuitable in many situations.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Easy" />
						<xs:enumeration value="Medium" />
						<xs:enumeration value="Fairly_hard" />
						<xs:enumeration value="Very_hard" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EcologicEffects">
		<xs:annotation>
			<xs:documentation>
				<Title>Ecological effects</Title>
				<Description>Element contains information on the Ecological effects which can be caused by the IntroducedAqSpecies
					of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Status" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Yes" />
						<xs:enumeration value="Probably_yes" />
						<xs:enumeration value="No" />
						<xs:enumeration value="Probably_no" />
						<xs:enumeration value="Unknown" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Beneficial" />
						<xs:enumeration value="Adverse" />
						<xs:enumeration value="Undecided" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EconData">
		<xs:annotation>
			<xs:documentation>
				<Title>Economic Data</Title>
				<Description>contains economic data for a country or sector. Use the Value attribute to enter the data value. Select
					the appropriate Class,
					Currency, Unit, Type and Commodity destination.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Class">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Commodity_balance" />
						<xs:enumeration value="Employment" />
						<xs:enumeration value="Gross_value_fisheries_output" />
						<xs:enumeration value="Trade" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Not_applicable" />
						<xs:enumeration value="Production" />
						<xs:enumeration value="Total_production" />
						<xs:enumeration value="Total_supply" />
						<xs:enumeration value="Per_caput_supply" />
						<xs:enumeration value="Trade_import" />
						<xs:enumeration value="Trade_export" />
						<xs:enumeration value="Sector_employment" />
						<xs:enumeration value="Secondary_employment" />
						<xs:enumeration value="Sector_secondary_employment" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Destination">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Not_applicable" />
						<xs:enumeration value="Human_consumption" />
						<xs:enumeration value="Animal_feed" />
						<xs:enumeration value="Industrial" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Unit">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Number_of_people" />
						<xs:enumeration value="Monetary_currency" />
						<xs:enumeration value="Live_weight_tons" />
						<xs:enumeration value="Kg-year" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:monetaryScale" />
			<xs:attribute name="Value" type="xs:positiveInteger" />
			<xs:attribute name="Date" type="xs:date" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EconomicReason">
		<xs:complexType mixed="false">
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="AnglingSport" />
						<xs:enumeration value="Aquaculture" />
						<xs:enumeration value="Ornamental" />
						<xs:enumeration value="Forage" />
						<xs:enumeration value="Fisheries" />
						<xs:enumeration value="Bait" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EconReport">
		<xs:annotation>
			<xs:documentation>
				<Title>Economic Report</Title>
				<Description>Referring to the main economic indicators provided in the document's introductory tables, and in
					particular the share of fisheries of
					the GDP, growth rate, share of fisheries exports/imports in the national
					foreign balance, gross value of fisheries output, consumption indexes,
					employment, human index development, ...a
					summarised overview of the performance of the fisheries sector is given. More focus on each of these
					major
					performance indicators is then provided in the sublevels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:EconData" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EcosystemAssessment">
		<xs:annotation>
			<xs:documentation>
				<Title>Ecosystem assessment</Title>
				<Description>xxxxx</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Results" />
				<xs:element ref="fi:RelatedAquaticResources" />
				<xs:element ref="fi:RelatedFisheries" />
				<xs:element ref="fi:FisheryRef" />
				<xs:element ref="fi:AqResRef" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Editor">
		<xs:annotation>
			<xs:documentation>
				<Title>Editor</Title>
				<Description>Editor contains the name of the person who edits a text but is not considered one of its authors.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Education">
		<xs:annotation>
			<xs:documentation>
				<Title>Education</Title>
				<Description>Includes presentation of educational programmes at all levels and their roles, from academic
					institutions to field vulgarisation
					projects. Use the fi:EducationEntry container for each instance of an
					educational activity.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:EducationEntry" />
				<xs:element ref="fi:OrgsInvolved" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EducationEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Education entry</Title>
				<Description>contains information about an educational activity.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:OrgOverview" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="EIMS">
		<xs:annotation>
			<xs:documentation>
				<Title>EIMS</Title>
				<Description>contains one or more EIMS queries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:EIMSEntry" />
				<xs:element ref="fi:PageSearch" />
				<xs:group ref="fi:textEntNoKOR" />
			</xs:choice>
			<xs:attribute name="Placeholder">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="NumberReturned" type="xs:int" />
			<xs:attribute name="ExecuteQuery" type="xs:boolean" />
