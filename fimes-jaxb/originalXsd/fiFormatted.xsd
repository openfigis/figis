	<xs:element name="FIGISDoc">
		<xs:annotation>
			<xs:documentation>
				<Title>figis Document</Title>
				<Description>the root element and root DTD for all FIGIS objects. Select FIGISDoc to begin a new XML document and
					then complete the required
					DataEntry and DocSource elements before selecting the desired domain in which to begin
					creating a new resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:DataEntry" />
				<xs:element ref="fi:ObjectSource" />
				<xs:choice>
					<xs:element ref="fi:AqRes" />
					<xs:element ref="fi:Collection" />
					<xs:element ref="fi:CountrySector" />
					<xs:element ref="fi:CultureSpecies" />
					<xs:element ref="fi:EAFTool" />
					<xs:element ref="fi:FishEquip" />
					<xs:element ref="fi:Fishery" />
					<xs:element ref="fi:FishTechnique" />
					<xs:element ref="fi:Geartype" />
					<xs:element ref="fi:LegalFramework" />
					<xs:element ref="fi:PortStateMeasure" />
					<xs:element ref="fi:Standard" />
					<xs:element ref="fi:Org" />
					<xs:element ref="fi:Species" />
					<xs:element ref="fi:Topic" />
					<xs:element ref="fi:VesselRecord" />
					<xs:element ref="fi:Vesseltype" />
					<xs:element ref="fi:VME" />
				</xs:choice>
				<xs:element ref="fi:Tags" minOccurs="0" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="FigisID">
		<xs:annotation>
			<xs:documentation>
				<Title>FIGIS identifier</Title>
				<Description> FigisID is the FIGIS generated identification number of the domain element which contains fi:FIGISId.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Object">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Object" />
						<xs:enumeration value="Observation" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="MetaID" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="FIGISSearch">
		<xs:annotation>
			<xs:documentation>
				<Title>FIGIS Search</Title>
				<Description>FIGISSearch is a query into the FIGIS data base.</Description>
				<Usage>The element "FIGISSearch" contains two main attributes: "Domain" and "Keyword". Use "Domain" to specify in
					which branch of the database the
					search should be performed. Use "Keyword" to specify in which tags of the XML files
					the search should be performed (e.g. name, code, country...).
					Not using "Domain" means that the search will be made
					across all domains while not using "Keyword" means that a full text search will be run.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Domain">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="area" />
						<xs:enumeration value="collection" />
						<xs:enumeration value="countrysector" />
						<xs:enumeration value="culturespecies" />
						<xs:enumeration value="eaftool" />
						<xs:enumeration value="equipment" />
						<xs:enumeration value="fishery" />
						<xs:enumeration value="fishtech" />
						<xs:enumeration value="geartype" />
						<xs:enumeration value="introsp" />
						<xs:enumeration value="legalframework" />
						<xs:enumeration value="psm" />
						<xs:enumeration value="ontology" />
						<xs:enumeration value="organization" />
						<xs:enumeration value="resource" />
						<xs:enumeration value="species" />
						<xs:enumeration value="topic" />
						<xs:enumeration value="vmstopic" />
						<xs:enumeration value="vmsprogramme" />
						<xs:enumeration value="vesseltype" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Keyword" />
			<xs:attribute name="Keyword1" />
			<xs:attribute name="Value" />
			<xs:attribute name="Value1" />
			<xs:attribute name="Operator">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="and" />
						<xs:enumeration value="or" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
		</xs:complexType>
	</xs:element>
