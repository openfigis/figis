	<xs:element name="FIGISCondLink">
		<xs:annotation>
			<xs:documentation>
				<Title>FIGIS Conditional Link</Title>
				<Description>FIGIS Conditional Link is a link which will be rendered only if a web resource is encountered.
					Typically this is done by querying the
					FIGIS web services.
				</Description>
				<Usage>The element "FIGISCondLink" contains several attributes: "Domain", "ID" and "Layout". Use "Domain" to specify
					in which branch of the
					database the search should be performed. Use "ID" to specify the file name or other
					identifier of the desired web resource (e.g. fact sheet:
					naso_albania, nalo_egypt, etc.). Use "Layout" for
					displaying the link in different ways (inline is the default display).</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Domain">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="area" />
						<xs:enumeration value="collection" />
						<xs:enumeration value="countrysector" />
						<xs:enumeration value="culturespecies" />
						<xs:enumeration value="eaftool" />
						<xs:enumeration value="equipment" />
						<xs:enumeration value="fishery" />
						<xs:enumeration value="fishtech" />
						<xs:enumeration value="geartype" />
						<xs:enumeration value="introsp" />
						<xs:enumeration value="legalframework" />
						<xs:enumeration value="psm" />
						<xs:enumeration value="ontology" />
						<xs:enumeration value="organization" />
						<xs:enumeration value="resource" />
						<xs:enumeration value="species" />
						<xs:enumeration value="topic" />
						<xs:enumeration value="vmstopic" />
						<xs:enumeration value="vmsprogramme" />
						<xs:enumeration value="vesseltype" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FAODomain">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="countryprofile" />
						<xs:enumeration value="faolex" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="CodeSystem">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="fao_area" />
						<xs:enumeration value="fao3alpha" />
						<xs:enumeration value="iso3" />
						<xs:enumeration value="FigisID" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ID" />
			<xs:attribute name="Layout">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Block" />
						<xs:enumeration value="Bullet" />
						<xs:enumeration value="Inline" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
		</xs:complexType>
	</xs:element>
	<xs:element name="FIGISCondLinkSet">
