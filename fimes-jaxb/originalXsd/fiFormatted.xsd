				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Collection" />
						<xs:enumeration value="production" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SelfMonitoring">
		<xs:annotation>
			<xs:documentation>
				<Title>SelfMonitoring</Title>
				<Description>contains information about SelfMonitoring.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Sequence">
		<xs:annotation>
			<xs:documentation>
				<Title>Sequence</Title>
				<Description>Sequence contains sequence numbers, text , and images about how a fishing technique is performed.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:SequenceNumber" />
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:FishTechniqueRef" />
				<xs:element ref="fi:VesseltypeRef" />
				<xs:element ref="fi:GeartypeRef" />
			</xs:choice>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Fishing" />
						<xs:enumeration value="Other" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SequenceNumber">
		<xs:annotation>
			<xs:documentation>
				<Title>Sequence Number</Title>
				<Description>SequenceNumber contains a sequence number for images related to a particular fishing technique.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Series">
		<xs:annotation>
			<xs:documentation>
				<Title>Series</Title>
				<Description>Series contains the name of the series of which an information resource is a part.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Sex">
		<xs:annotation>
			<xs:documentation>
				<Title>Sex</Title>
				<Description>Sex contains the fi:Sex of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Shipyard">
		<xs:annotation>
			<xs:documentation>
				<Title>Shipyard</Title>
				<Description>is the name and location of the organisation that built the vessel.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="6" />
				<xs:element ref="fi:LandAreaRef" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SiteIdentification">
		<xs:annotation>
			<xs:documentation>
				<Title>SiteIdentification</Title>
				<Description>contains information about procedures involved in identifying a suitable site in which to engage in
					aquaculture.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Size">
		<xs:annotation>
			<xs:documentation>
				<Title>Size</Title>
				<Description>Size is species' size information. Can be length or weight information. May also be linked to fi:Age
					and fi:Sex. Dimensions may be
					ranges or minimums or maximums.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Sex" />
				<xs:element ref="fi:Age" />
				<xs:element ref="fi:Min" />
				<xs:element ref="fi:Max" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SocioEconomic">
		<xs:annotation>
			<xs:documentation>
				<Title>Socioeconomic</Title>
				<Description>SocioEconomic contains information about the economic benefits/costs of a fishing technique.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SocioEconomicAssessment">
		<xs:annotation>
			<xs:documentation>
				<Title>Socioeconomic assessment</Title>
				<Description>xxxxx</Description>
				<Usage Domain="Fishery"> is a container for describing the socioeconomic assessment of a fishery.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Data" />
				<xs:element ref="fi:Results" />
				<xs:element ref="fi:RelatedFisheries" />
				<xs:element ref="fi:FisheryRef" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SocioEconomicEffects">
		<xs:annotation>
			<xs:documentation>
				<Title>Socio-Economical Effects</Title>
				<Description>Element contains information on the Socio-Economical effects which can be caused by the
					IntroducedAqSpecies of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Status" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Yes" />
						<xs:enumeration value="Probably_yes" />
						<xs:enumeration value="No" />
						<xs:enumeration value="Probably_no" />
						<xs:enumeration value="Unknown" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Beneficial" />
						<xs:enumeration value="Adverse" />
						<xs:enumeration value="Undecided" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SocioEconReport">
		<xs:annotation>
			<xs:documentation>
				<Title>Socio-Economic Report</Title>
				<Description>Referring to the main economic indicators provided in the document's introductory tables, and in
					particular the share of fisheries of
					the GDP, growth rate, share of fisheries exports/imports in the national
					foreign balance, gross value of fisheries output, consumption indexes,
					employment, human index development, ...a
					summarised overview of the performance of the fisheries sector is given. More focus on each of these
					major
					performance indicators is then provided in the sublevels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:EconData" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Solution">
		<xs:annotation>
			<xs:documentation>
				<Title>Solution</Title>
				<Description>contains an instance of a solution.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Solutions">
		<xs:annotation>
			<xs:documentation>
				<Title>Solutions</Title>
				<Description>is a container for information about the fisheries solutions with which an organisation plans to tackle
					its issues or problems.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Solution" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Sources">
		<xs:annotation>
			<xs:documentation>
				<Title>Source of information</Title>
				<Description>Sources is a container for biblio entry elements. It is used to identify the source of a piece of data
					within an object when it is
					not part of a bibliography. For example, an editor inserting a table of data from a FAO
					dataset into the middle of an object might cite FAO using
					fi:Sources. Use an instance of fi:BiblioEntry for each
					separate citation. Use fi:BiblioEntryLink to point a source citation to an existing
					fi:BiblioEntry anywhere in the
					FIGISDoc. fi:Sources contains a list of fi:BiblioEntry. fi:Sources can be found at any level where there is not an
					fi:ObjectSource.
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:BiblioEntry" />
			</xs:choice>
			<xs:attribute name="RefersTo">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Block" />
						<xs:enumeration value="Entire_file" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SpatialScale">
		<xs:annotation>
			<xs:documentation>
				<Title>Spatial scale</Title>
				<Description>Geographic level at which the resource is monitored. In Fishery domain it is the geographic scale
					associated with the Geographic
					reference.</Description>
				<Usage>Spatial scale contains a standard term such as Global, Regional (e.g. for the whole Atlantic), sub-regional
					(e.g. for a part of the
					Atlantic), national, local (for sub-national levels).</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Global" />
						<xs:enumeration value="Regional" />
						<xs:enumeration value="Sub-Regional" />
						<xs:enumeration value="National" />
						<xs:enumeration value="Local" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SpecialisationIntegration">
		<xs:annotation>
			<xs:documentation>
				<Title>Specialisation and Integration</Title>
				<Description>contains information about aquaculture specialisation and integration.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Species">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Species</Title>
				<Description>Species is the Aquatic Species domain root element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:SpeciesIdent" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:OrigDesc" />
					<xs:element ref="dwc:identificationReferences" minOccurs="0" />
					<xs:element ref="fi:Synonym" />
					<xs:element ref="fi:Misidentification" />
					<xs:element ref="fi:DiagnosticFeat" />
					<xs:element ref="fi:GeoDist" />
					<xs:element ref="fi:HabitatBio" />
					<xs:element ref="fi:InterestFisheries" />
					<xs:element ref="fi:LocalName" />
					<xs:element ref="fi:AddInfo" />
					<xs:element ref="fi:ConservStatus" />
					<xs:element ref="fi:ThreatToHumans" />
					<xs:group ref="fi:indexEnt" minOccurs="0" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SpeciesIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Species Identity</Title>
				<Description>SpeciesIdent contains the necessary elements of positive species identification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:SpeciesRef" />
				<xs:element ref="fi:Image" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attributeGroup ref="fi:statusEnt" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SpeciesList">
		<xs:annotation>
			<xs:documentation>
				<Title>Species list</Title>
				<Description>contains a list of species references with optionally an ad hoc title for the corresponding group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:SpeciesRef" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Associated" />
						<xs:enumeration value="Captured" />
						<xs:enumeration value="Discarded" />
						<xs:enumeration value="Protected" />
						<xs:enumeration value="Target" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SpeciesName">
		<xs:annotation>
			<xs:documentation>
				<Title>Species</Title>
				<Description>SPE is species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Taxonomic">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Taxonomic" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SpeciesRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Species reference</Title>
				<Description>Element used to refer to a Species domain element. It contains various species classification systems
					(taxonomic, ISSCAAP, ...) and
					codes (including scientific names) used for positive referencing of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="1" maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" minOccurs="0" maxOccurs="1" />
				<xs:element ref="fi:ForeignID" minOccurs="0" maxOccurs="unbounded" />
				<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6" />
				<xs:element ref="dwc:Taxon" minOccurs="0" maxOccurs="1" />
				<xs:element ref="fi:SciName" minOccurs="0" />
				<xs:element ref="fi:FAOName" minOccurs="0" />
				<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded" />
			</xs:choice>
			<xs:attribute name="Taxonomy">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Genus" />
						<xs:enumeration value="Species" />
						<xs:enumeration value="Tribe" />
						<xs:enumeration value="SubFamily" />
						<xs:enumeration value="Family" />
						<xs:enumeration value="SubOrder" />
						<xs:enumeration value="Order" />
						<xs:enumeration value="Class" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Staff">
		<xs:annotation>
			<xs:documentation>
				<Title>Staff</Title>
				<Description>contains name, title, competence and contact information for fisheries organisation staff.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Stakeholders">
		<xs:annotation>
			<xs:documentation>
				<Title>Stakeholders</Title>
				<Description>Stakeholders contains information about the interested parties or the objects of an organisation or
					management system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Fishing_community" />
						<xs:enumeration value="Industry" />
						<xs:enumeration value="Governmental_organisation" />
						<xs:enumeration value="NGO" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Standard">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Standard</Title>
				<Description>This is the root element for the domain and contains three levels of complexity:Select the Text
					container for lightweight
					description. It allows for text, formatting, and simple elements related to the
					domain.Select the Ref container for positive linking to an
					existing database object. This will cause the FIGIS
					database loader to link to the existing object so that any description in the document will
					be complemented by the
					existing information in the database, though it will NOT alter the existing contents of the referenced database
					object.Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:StandardIdent" />
				<xs:element ref="fi:StandardProfile" minOccurs="0" />
				<xs:group ref="fi:indexEnt" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="CTN" type="xs:positiveInteger" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardAbundanceLevel">
		<xs:annotation>
			<xs:documentation>
				<Title>FIRMS Standard Abundance Level</Title>
				<Description>xxxxx</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Pre-exploitation biomass or high abundance" />
						<xs:enumeration value="Intermediate abundance" />
						<xs:enumeration value="Low abundance" />
						<xs:enumeration value="Depleted" />
						<xs:enumeration value="Uncertain/Not assessed" />
						<xs:enumeration value="Not applicable" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardChild">
		<xs:annotation>
			<xs:documentation>
				<Title>Standard Child</Title>
				<Description>contains a reference to Child Standards.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:StandardRef" maxOccurs="unbounded" />
				<xs:element ref="fi:ClassificationSystem" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardExploitRate">
		<xs:annotation>
			<xs:documentation>
				<Title>FIRMS Standard Exploitation Rate</Title>
				<Description>xxxxx</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="No or low fishing mortality" />
						<xs:enumeration value="Moderate fishing mortality" />
						<xs:enumeration value="High fishing mortality" />
						<xs:enumeration value="Uncertain/Not assessed" />
						<xs:enumeration value="Not applicable" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Standard Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of
					this element in a document
					causes the FIGIS database loader to create a database object that will be keyed using the
					information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="6" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element ref="fi:Acronym" />
						<xs:element ref="fi:StandardParent" />
					</xs:choice>
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardParent">
		<xs:annotation>
			<xs:documentation>
				<Title>Standard Parent</Title>
				<Description>contains a reference to a parent Standard.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:StandardRef" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardProfile">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Standard Profile</Title>
				<Description>Profile contains the elements used by domain specialists to profile a domain object and declare it as a
					unique type. That is, rather
					than containing the Identifying codes or names (Keys) for the object as in Ident, the
					Profile block contains the information that is intrinsic
					and invariable to a description of the object, e.g. for an
					aquatic species this would be elements such as fieldmarks or diagnostic features.
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Rule" />
				<xs:element ref="fi:Instance" />
				<xs:element ref="fi:ClassificationSystem" />
				<xs:element ref="fi:StandardChild" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StandardRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Standard Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of
					this element in a document
					causes the FIGIS database loader to create a database object that will be keyed using the
					information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="7">
				<xs:element ref="fi:FigisID" />
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:Acronym" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="StateOfActivity">
		<xs:annotation>
			<xs:documentation>
				<Title>State of Activity</Title>
				<Description>refers to the state of an activity of an Isntitution, Organization, Projects or Activity.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Planned" />
						<xs:enumeration value="Ongoing" />
						<xs:enumeration value="Completed" />
						<xs:enumeration value="Cancelled" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StatisticEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Statistic entry</Title>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="dc:Title" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Page" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Statistics">
		<xs:annotation>
			<xs:documentation>
				<Title>Statistics</Title>
				<Description>contains statistics and associated information related to the exploitation of the aquatic resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:History" />
				<xs:element ref="fi:StatisticEntry" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StatusTrendEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>FI-Topic Trend entry</Title>
				<Description>contains general text and low-level elements for a Fisheries trend.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="StatusValue" />
			<xs:attribute name="TrendValue" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StatusTrends">
		<xs:annotation>
			<xs:documentation>
				<Title>Trends, Issues and Development</Title>
				<Description>Overview of the status and trends of the fisheries sector, highlighting the main issues faced and to be
					addressed. Attention will in
					particular be provided on the state of resources and supply and demand (for next 25
					years). If desired, use a new fi:StatusTrendEntry element for
					each aspect.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:StatusTrendEntry" />
				<xs:element ref="fi:Issues" />
				<xs:element ref="fi:Strategies" />
				<xs:element ref="fi:Constraints" />
				<xs:element ref="fi:ResearchEducation" />
				<xs:element ref="fi:AidOrInvestments" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="StatusValue" />
			<xs:attribute name="TrendValue" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StorageMethod">
		<xs:annotation>
			<xs:documentation>
				<Title>Storage Method</Title>
				<Description>contains information about a vessel's method of storing catch.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Strategies">
		<xs:annotation>
			<xs:documentation>
				<Title>Management strategies</Title>
				<Description>Management Strategies are strategies related to missions, outputs, projects, etc. Use one instance of
					fi:Strategy for each instance
					of a Strategy.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:StrategyEntry" />
				<xs:element ref="fi:RelatedFisheries" />
			</xs:choice>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="StrategyEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Strategy entry</Title>
				<Description>Use one fi:StrategyEntry for each instance of a strategy.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="fi:textEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SubFamily">
		<xs:annotation>
			<xs:documentation>
				<Title>Subfamily</Title>
				<Description>SUBFAM is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Taxonomic">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Taxonomic" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:asfis" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SubOrder">
		<xs:annotation>
			<xs:documentation>
				<Title>Sub-order</Title>
				<Description>SUBORD is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Taxonomic">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Taxonomic" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:asfis" />
			<xs:attribute name="OtherType" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Subset">
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Key" />
				<xs:element ref="fi:Dimension" />
				<xs:element ref="fi:DataSource" />
				<xs:element ref="fi:QualityAssurance" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SupplyAndDemand">
		<xs:annotation>
			<xs:documentation>
				<Title>Supply and Utilisation</Title>
				<Description>contains supply and demand information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SupportIndustry">
		<xs:annotation>
			<xs:documentation>
				<Title>Supporting Industries</Title>
				<Description>contains information about industries that support aquaculture production.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SurfaceArea">
		<xs:annotation>
			<xs:documentation>
				<Title>Surface Area</Title>
				<Description>SurfaceArea is the surface area of an object. Tag the value and define the fi:Unit attribute.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:areaUnit" />
			<xs:attribute name="Value" type="xs:positiveInteger" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Synonym">
		<xs:annotation>
			<xs:documentation>
				<Title>Synonym</Title>
				<Description>Synonym is a container for the scientific names that exist for a species but are no longer valid. Use
					fi:SynEntry for each instance.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:SynonymEntry" />
			</xs:choice>
			<xs:attribute name="Type" default="S">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="S" />
						<xs:enumeration value="O" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="SynonymEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Synonym entry</Title>
				<Description>SynonymEntry contains a species synonym together with any accompanying notes.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dwc:Taxon" minOccurs="0" />
				<xs:group ref="fi:textEnt" minOccurs="0" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Table">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="Title" type="xs:string" />
			<xs:attribute name="ID" type="xs:string" />
			<xs:attribute name="IDRef" type="xs:string" />
			<xs:attribute name="DisplayType">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Inline" />
						<xs:enumeration value="Link" />
						<xs:enumeration value="Popup" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Description" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Tag">
		<xs:annotation>
			<xs:documentation>
				<Title>Tag</Title>
				<Description>A tag is a non-hierarchical keyword or term assigned to a piece of information. This kind of metadata
					helps describe an item and
					allows it to be found again by browsing or searching.</Description>
				<Usage>Select a fixed Value among the controlled list or submit a free value. Tag should be always in capital.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="required" />
			<xs:attribute ref="xml:lang" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Tags">
		<xs:annotation>
			<xs:documentation>
				<Title>Tags</Title>
				<Description>A container for tags used to describe a FIGIS Document.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:Tag" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TargetSpecies">
		<xs:annotation>
			<xs:documentation>
				<Title>Target Species</Title>
				<Description>Those species that are primarily sought by the fishermen in a particular fishery. The subject of
					directed fishing effort in a
					fishery. There may be primary as well as secondary target species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:SpeciesList" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TargetSpeciesEnviron">
		<xs:annotation>
			<xs:documentation>
				<Title>Fishing Technique Target Species and its Environment</Title>
				<Description>TargetSpeciesEnviron contains information about a target species and its environment.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:TargetSpecies" />
				<xs:element ref="fi:FishingEnviron" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Telephone">
		<xs:annotation>
			<xs:documentation>
				<Title>Telephone</Title>
				<Description>Telephone contains the telephone number for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="CountryPrefix" type="xs:positiveInteger" />
			<xs:attribute name="AreaPrefix" type="xs:positiveInteger" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Telex">
		<xs:annotation>
			<xs:documentation>
				<Title>Telex</Title>
				<Description>Telex contains the telex number for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TermsAndConditions">
		<xs:annotation>
			<xs:documentation>
				<Title>TermsAndConditions</Title>
				<Description>contains information about contractual requirements related to the authorisation to engage in
					aquaculture and/or fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Text">
		<xs:annotation>
			<xs:documentation>
				<Title>Text block</Title>
				<Description>Text container, which might include an optional Title, as well as HTML formatting tags within the CDATA
					text string element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="Title" type="xs:string" />
			<xs:attribute ref="xml:lang" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ThematicApproach">
		<xs:annotation>
			<xs:documentation>
				<Title>Thematic Approach</Title>
				<Description>"The thematic approach highlights the disciplinary viewpoint prevailing in the
					identification of fishery
					units" (FIRMS TWG2, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Fishery Resource" />
						<xs:enumeration value="Jurisdictional" />
						<xs:enumeration value="Production System" />
						<xs:enumeration value="Fishery Management Unit" />
						<xs:enumeration value="Fishing Activity" />
						<xs:enumeration value="Access Rights" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ThreatToHumans">
		<xs:annotation>
			<xs:documentation>
				<Title>Threat to Humans</Title>
				<Description>ThreatToHumans is a review of the possible aggressive behaviour of a species toward humans.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TimeRange">
		<xs:annotation>
			<xs:documentation>
				<Title>Time Range</Title>
				<Description>XXXXX</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:Range" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Tonnage">
		<xs:annotation>
			<xs:documentation>
				<Title>Tonnage</Title>
				<Description>Tonnage is a measurement of a vessel's capacity. The gross tonnage is the overall volume of a ship's
					hull, including crew cabins,
					storerooms and machinery spaces. GRT is the gross registered tonnage of the vessel as
					entered on the national reigster or other record of
					vessels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Net" />
						<xs:enumeration value="Gross" />
						<xs:enumeration value="GRT" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Unit" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Register_tons" />
						<xs:enumeration value="Metric_tons" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Value" type="xs:positiveInteger" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Topic">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic</Title>
				<Description>This is the root element for the domain and contains three levels of complexity:Select the Text
					container for lightweight
					description. It allows for text, formatting, and simple elements related to the
					domain.Select the Ref container for positive linking to an
					existing database object. This will cause the FIGIS
					database loader to link to the existing object so that any description in the document will
					be complemented by the
					existing information in the database, though it will NOT alter the existing contents of the referenced database
					object.Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:TopicIdent" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:Overview" />
					<xs:element ref="fi:Issues" />
					<xs:element ref="fi:TopicImages" />
					<xs:group ref="fi:indexEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute name="CTN" type="xs:positiveInteger" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TopicCoverage">
		<xs:annotation>
			<xs:documentation>
				<Title>Topic Coverage</Title>
				<Description>the topics with which an organisation is concerned.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:TopicRef" />
				<xs:element ref="fi:TopicCoverageType" />
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TopicCoverageType">
		<xs:annotation>
			<xs:documentation>
				<Title>Topic Coverage Type</Title>
				<Description>the type of the topic coverage.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Aquaculture" />
						<xs:enumeration value="Conservation" />
						<xs:enumeration value="Environment" />
						<xs:enumeration value="GIS/Remote_sensing" />
						<xs:enumeration value="Inland_fisheries" />
						<xs:enumeration value="Marine_fisheries" />
						<xs:enumeration value="Recreational_fisheries" />
						<xs:enumeration value="Social_issues" />
						<xs:enumeration value="Aquaculture statistics" />
						<xs:enumeration value="Artisanal_fisheries" />
						<xs:enumeration value="Capture_fisheries" />
						<xs:enumeration value="Ecosystem_approach" />
						<xs:enumeration value="Fisheries_governance" />
						<xs:enumeration value="Fisheries_statistics" />
						<xs:enumeration value="Responsible_fisheries" />
						<xs:enumeration value="Small-scale_fisheries" />
						<xs:enumeration value="Trade" />
						<xs:enumeration value="Utilization" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TopicIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of
					this element in a document
					causes the FIGIS database loader to create a database object that will be keyed using the
					information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" />
				<xs:sequence>
					<xs:element ref="dc:Title" maxOccurs="6" />
					<xs:element ref="fi:TopicParent" minOccurs="0" />
					<xs:element ref="fi:ForeignID" minOccurs="0" />
				</xs:sequence>
			</xs:choice>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" use="optional" />
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Topic" />
						<xs:enumeration value="Issue" />
						<xs:enumeration value="Gateway" />
						<xs:enumeration value="Activity" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TopicImages">
		<xs:annotation>
			<xs:documentation>
				<Title>Images</Title>
				<Description>can hold text and/or other images for a gallery.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="TopicParent">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic Parent</Title>
				<Description>is a container for the reference to a topic parent.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:TopicRef" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TopicRef">
		<xs:annotation>
			<xs:documentation>
				<Title>FI-Topic reference</Title>
				<Description>contains positive ID References to the fi-topic domain.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" />
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:ForeignID" />
				<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Trade">
		<xs:annotation>
			<xs:documentation>
				<Title>Trade</Title>
				<Description>contains information about fishing trade. Use fi:Tradeflows for import/export text.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:Tradeflows" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TradeflowEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Tradeflow entry</Title>
				<Description>contains information about fishing trade flows. Use the Type attribute to delimit the tag.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:EndMarket" />
				<xs:element ref="fi:Origin" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Volume" />
						<xs:enumeration value="Value" />
						<xs:enumeration value="Volume_and_value" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Tradeflows">
		<xs:annotation>
			<xs:documentation>
				<Title>Tradeflows</Title>
				<Description>contains information about fishing trade flows. Use fi:TradeflowEntry for individual tradeflows.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:TradeflowEntry" />
			</xs:choice>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Import" />
						<xs:enumeration value="Export" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Tribe">
		<xs:annotation>
			<xs:documentation>
				<Title>Tribe</Title>
				<Description>Tribe is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Taxonomic">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Taxonomic" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:asfis" />
		</xs:complexType>
	</xs:element>
	<xs:element name="TripDuration">
		<xs:annotation>
			<xs:documentation>
				<Title>Trip duration</Title>
				<Description>"Average duration of fishing trip specifying in which unit it is expressed" (Fisheries Inventory:
					Method and Guidelines. FIRMS,
					2007).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="TypeOfProductionSystem">
		<xs:annotation>
			<xs:documentation>
				<Title>Type of Production System</Title>
				<Description>the scale at which a fishery operates.</Description>
				<Usage>is a container for indicating the scale at which a fishery operates (e.g. Artisanal-Small Scale, Commercial-
					Small Scale, Semi industrial).</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="FreeValue" use="optional" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
			<xs:attribute name="Class">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Large-scale" />
						<xs:enumeration value="Small-scale" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:sectorEnt" />
			<xs:attribute name="Value">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Subsistence" />
						<xs:enumeration value="Recreational" />
						<xs:enumeration value="Commercial" />
						<xs:enumeration value="Artisanal" />
						<xs:enumeration value="Semi-industrial" />
						<xs:enumeration value="Industrial" />
						<xs:enumeration value="Exploratory_fishery" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="TypicalUsage">
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Uncertainty">
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="High" />
						<xs:enumeration value="Intermediate" />
						<xs:enumeration value="Low" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Unit">
		<xs:annotation>
			<xs:documentation>
				<Title>Unit</Title>
				<Description>contains the name of a measurement unit.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="ValidationMechanism">
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Value">
		<xs:annotation>
			<xs:documentation>
				<Title>Value</Title>
				<Description>contains a value.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Range" />
				<xs:element ref="fi:Avg" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VerticalDist">
		<xs:annotation>
			<xs:documentation>
				<Title>Vertical distribution</Title>
				<Description>"Marine habitat: type of habitat describing the fishery resource distribution according to its
					relationship with the sea bottom."
					(FIRMS Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" use="optional" />
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Demersal/Benthic" />
						<xs:enumeration value="Demersal" />
						<xs:enumeration value="Benthic" />
						<xs:enumeration value="Pelagic" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FreeValue" use="optional" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselActive">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Active</Title>
				<Description>is a container for registry status information on active vessels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute name="DateAuthorised" type="xs:date" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselOperator">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Operator</Title>
				<Description>contains the name and/or contact information for a vessel operator. It means the owner of bareboat
					charterer, or any other natural or
					juridical person to whom the responsibilities of the owner or bareboat charterer
					have been formally assigned</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselOwner">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Owner</Title>
				<Description>contains the name and/or contact information for a vessel owner. It means any natural or juridical
					person recorded in the register of
					ships of the State of registration as an owner of a shipcontains the name and/or
					contact information for a vessel owner.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRecord">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord</Title>
				<Description>This is the root element for the domain and contains three levels of complexity:Select the Text
					container for lightweight
					description. It allows for text, formatting, and simple elements related to the
					domain.Select the Ref container for positive linking to an
					existing database object. This will cause the FIGIS
					database loader to link to the existing object so that any description in the document will
					be complemented by the
					existing information in the database, though it will NOT alter the existing contents of the referenced database
					object.Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:VesselRecordIdent" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:VesselRecordProfile" />
					<xs:element ref="fi:VesselRecordFeature" />
				</xs:choice>
				<xs:group ref="fi:indexEnt" minOccurs="0" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRecordFeature">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord Feature</Title>
				<Description>is a container for supplementary elements related to the object but not essential to its Identification
					or distinction, e.g. for an
					aquatic species this would be elements such as conservation status or interest to
					fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:VesselReport" />
				<xs:element ref="fi:VesselRegistryStatus" />
				<xs:element ref="fi:Infringement" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRecordIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of
					this element in a document
					causes the FIGIS database loader to create a database object that will be keyed using the
					information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" />
				<xs:sequence maxOccurs="unbounded">
					<xs:element ref="fi:VesselRecordIntID" maxOccurs="unbounded" />
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element ref="fi:Built" />
						<xs:element ref="fi:Image" />
					</xs:choice>
				</xs:sequence>
			</xs:choice>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRecordIntID">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord International Id</Title>
				<Description>contains international classification codes and related information for VesselRecord
					Identification.IMOID is the vessel Identifying
					code assigned by Lloyd's Register of Shipping, a classification
					society based in the United Kingdom. Permanent number assigned to ships for
					Identification purposes. It is made of
					the three letters IMO in front of the Lloyd's Register of Shipping (Lr) Number.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:IMOConfidence" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="CodeSystem" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="HSVAR" />
						<xs:enumeration value="IMO" />
						<xs:enumeration value="InMarSat" />
						<xs:enumeration value="RFB" />
						<xs:enumeration value="EU" />
						<xs:enumeration value="Other" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Source" type="xs:string" />
			<xs:attributeGroup ref="fi:code" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRecordNatID">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord National Id</Title>
				<Description>contains the Identifying vessel's codes valid at a national or regional, but not global, level. These
					codes are subject to changes if
					the vessel operates under different flag.Callsign contains the Identifying code
					letters or numbers used for Identification during radio
					transmission. This is a unique identifier allocated to each
					ship by its national competent authority. Each country is allocated, by the
					International telecommunication union
					(Itu), codes beginning with specific characters. The first two characters are two letters or a letter
					followed by a
					digit or a digit followed by a letter. The first two characters or in certain cases the first character of a call
					sign constitute
					the nationality Identification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attributeGroup ref="fi:code" />
			<xs:attribute name="CodeSystem" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Name" />
						<xs:enumeration value="Callsign" />
						<xs:enumeration value="RegNumber" />
						<xs:enumeration value="Flagstate" />
						<xs:enumeration value="Other" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRecordProfile">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord Profile</Title>
				<Description>Profile contains the elements used by domain specialists to profile a domain object and declare it as a
					unique type. That is, rather
					than containing the Identifying codes or names (Keys) for the object as in Ident, the
					Profile block contains the information that is intrinsic
					and invariable to a description of the object, e.g. for an
					aquatic species this would be elements such as fieldmarks or diagnostic features.
				</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:VesselSpecs" />
				<xs:element ref="fi:VesseltypeRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselRegistryStatus">
		<xs:annotation>
			<xs:documentation>
				<Title>High Seas Vessel Registry Status</Title>
				<Description>VESSELREGISTRY is a container for registry status information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Date" />
				<xs:element ref="fi:Sources" />
				<xs:choice>
					<xs:element ref="fi:VesselActive" />
					<xs:element ref="fi:VesselSuspended" />
				</xs:choice>
				<xs:choice minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselReport">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Report</Title>
				<Description>is a container for descriptive elements related to a Vessel which are subject to change.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="dc:Date" />
				<xs:element ref="fi:VesselRecordNatID" />
				<xs:element ref="fi:PortOfCall" />
				<xs:element ref="fi:Province" />
				<xs:element ref="fi:VesselOwner" />
				<xs:element ref="fi:VesselOperator" />
				<xs:element ref="fi:GeartypeRef" />
				<xs:element ref="fi:StorageMethod" />
				<xs:element ref="fi:WheelhEquip" />
				<xs:element ref="fi:Highlight" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselSold">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Sold</Title>
				<Description>is a container for registry status information for vessels that have been sold.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:LandAreaRef" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselSpecs">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Technical Specifications</Title>
				<Description>contains a group of elements detailing the technical specifications of a Vessel.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:VesseltypeLength" />
				<xs:element ref="fi:VesseltypeDepth" />
				<xs:element ref="fi:Draught" />
				<xs:element ref="fi:Beam" />
				<xs:element ref="fi:PowerClass" />
				<xs:element ref="fi:BuildMaterial" />
				<xs:element ref="fi:Tonnage" />
				<xs:element ref="fi:HoldCapacity" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselSuspended">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Suspended</Title>
				<Description>is a container for registry status information on suspended vessels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:VesselSold" />
			</xs:choice>
			<xs:attribute name="DateAuthorised" type="xs:date" />
			<xs:attribute name="DateSuspended" type="xs:date" />
			<xs:attribute name="ReasonSuspended" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration
							value="Voluntary_relinquishment_or_non-renewal_of_the_fishing_authorization_by_the_fishing_vessel_owner_or_operator" />
						<xs:enumeration
							value="Withdrawal_of_the_fishing_authorization_issued_in_respect_of_the_fishing_vessel_under_paragraph_8_of_articleIii" />
						<xs:enumeration value="Fishing_vessel_concerned_is_no_longer_entitled_to_fly_its_flag" />
						<xs:enumeration value="Scrapping_decommissioning_or_loss_of_the_fishing_vessel_concerned" />
						<xs:enumeration value="Any_other_reason" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Vesseltype">
		<xs:annotation>
			<xs:documentation>
				<Title>Vesseltype</Title>
				<Description>This is the root element for the domain. It allows for text, formatting, and simple elements related to
					the domain. Select the Ref
					container for positive linking to an existing database object. This will cause the FIGIS
					database loader to link to the existing object so that
					any description in the document will be complemented by the
					existing information in the database, though it will NOT alter the existing contents
					of the referenced database
					object. Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:VesseltypeIdent" />
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:Overview" />
					<xs:element ref="fi:GeartypeOverview" />
					<xs:element ref="fi:FishTechniqueOverview" />
					<xs:element ref="fi:FishEquipOverview" />
					<xs:element ref="fi:DeckType" />
					<xs:element ref="fi:VesseltypeClass" />
					<xs:element ref="fi:DeckArrange" />
					<xs:element ref="fi:CatchHandlingProcEquip" />
					<xs:element ref="fi:VesseltypeEquip" />
					<xs:group ref="fi:indexEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeClass">
		<xs:annotation>
			<xs:documentation>
				<Title>VesseltypeClass</Title>
				<Description>VesseltypeClass is a vessel classification grouping.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:VesseltypeLength" />
				<xs:element ref="fi:PowerClass" />
				<xs:element ref="fi:Tonnage" />
				<xs:element ref="fi:AddInfo" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeDepth">
		<xs:annotation>
			<xs:documentation>
				<Title>Vesseltype Depth</Title>
				<Description>VesseltypeDepth is a measurement from the underside of a vessel's deck to the bottom of its hold.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attributeGroup ref="fi:lengthUnit" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeEquip">
		<xs:annotation>
			<xs:documentation>
				<Title>Equipment</Title>
				<Description>VesseltypeEquip is a container for vessel equipment elements.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:DeckEquip" />
				<xs:element ref="fi:WheelhEquip" />
				<xs:element ref="fi:FishDetEquip" />
				<xs:element ref="fi:GeartypeOverview" />
				<xs:element ref="fi:CatchHandlingProcEquip" />
				<xs:element ref="fi:SecuEquip" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>Vesseltype Identity</Title>
				<Description>VesseltypeIdent contains the core classification codes and related information for positive vessel type
					identification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" />
				<xs:sequence>
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="fi:ISSCFVCode" />
						<xs:element ref="fi:FIDICode" />
					</xs:choice>
					<xs:element ref="dc:Title" maxOccurs="3" />
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="fi:Image" minOccurs="0" />
						<xs:element ref="dc:Description" />
					</xs:choice>
				</xs:sequence>
			</xs:choice>
			<xs:attributeGroup ref="fi:statusEnt" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeLength">
		<xs:annotation>
			<xs:documentation>
				<Title>VesseltypeLength</Title>
				<Description>is a measurement of a boat's longest dimension. For purposes of the HSVAR agreement the attribute
					ComplianceType has been added. TYPE
					1 for any fishing vessel built after 18 July 1982, 96 percent of the total
					length on a waterline at 85 percent of the least moulded depth
					measured from the top of the keel, or the length from
					the foreside of the stern to the axis of the rudder stock on that waterline, if that be
					greater. In ships designed
					with a rake of keel the waterline on which this length is measured shall be parallel to the designed waterline TYPE
					2
					For any fishing vessel built before 18 July 1982, registered length as entered on the national register or other
					record of vessels</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="LOA" />
						<xs:enumeration value="Actual" />
						<xs:enumeration value="Registered" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:lengthUnit" />
			<xs:attribute name="ComplianceType">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Pre82" />
						<xs:enumeration value="Post82" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeOverview">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel type overview</Title>
				<Description>VesseltypeOverview contains high-level information about one instance of one vesseltype. Use a new
					element for each vesseltype.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:GeartypeRef" />
				<xs:element ref="fi:VesseltypeRef" />
				<xs:element ref="fi:FishTechniqueRef" />
				<xs:element ref="fi:FlagStateList" />
				<xs:element ref="fi:VesseltypeClass" />
				<xs:element ref="fi:Crew" />
				<xs:element ref="fi:CatchHandlingProcEquip" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesseltypeRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel type reference</Title>
				<Description>is used to positively identify vessel types. ISSSCFV or FIES code is required. Select a classification
					system, and enter the value in
					'Code'.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:ForeignID" />
				</xs:choice>
				<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6" />
				<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselUnderJurisdiction">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselUnderJurisdiction</Title>
				<Description>contains information about the jurisdiction on which the vessel is targeted by a measure.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:VesselUnderJurisdictionType" />
				<xs:element ref="fi:FlagStateList" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VesselUnderJurisdictionType">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselUnderJurisdictionType</Title>
				<Description>"The flag or status of the vessel targeted by the measure" (LEGN, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:FlagStateList" />
				<xs:element ref="fi:OrgRef" />
			</xs:choice>
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="All_vessels" />
						<xs:enumeration value="National_vessels" />
						<xs:enumeration value="Foreign_vessels" />
						<xs:enumeration value="Foreign_flag" />
						<xs:enumeration value="RFMO" />
						<xs:enumeration value="Non-RFMO" />
						<xs:enumeration value="EU_vessels" />
						<xs:enumeration value="Non-EU_vess" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" use="optional" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VME">
		<xs:annotation>
			<xs:documentation>
				<Title>VME</Title>
				<Description>Vulnerable Marine Ecosystem (VME)</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:VMEIdent" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:Overview" />
					<xs:element ref="fi:HabitatBio" />
					<xs:element ref="fi:Impacts" />
					<xs:element ref="fi:History" />
					<xs:element ref="fi:FisheryHistory" />
					<xs:element ref="fi:Management" />
					<xs:element ref="fi:AddInfo" />
					<xs:group ref="fi:indexEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VMECriteria">
		<xs:annotation>
			<xs:documentation>
				<Title>VME Criteria</Title>
				<Description>XXXX</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="optional">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Uniqueness or rarity" />
						<xs:enumeration value="Functional significance of the habitat" />
						<xs:enumeration value="Fragility" />
						<xs:enumeration value="Life-history traits" />
						<xs:enumeration value="Structural complexity" />
						<xs:enumeration value="Unspecified" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VMEIdent">
		<xs:annotation>
			<xs:documentation>
				<Title>VME identity</Title>
				<Description>VMEIdent contains identity information about VME.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:WaterAreaRef" />
					<xs:element ref="fi:OrgRef" />
					<xs:element ref="dc:Title" />
					<xs:element ref="fi:VMEType" />
					<xs:element ref="fi:VMECriteria" />
					<xs:element ref="fi:ReportingYear" maxOccurs="1" />
					<xs:element ref="fi:Range" minOccurs="0" />
					<xs:element ref="fi:GeoReference" />
				</xs:choice>
			</xs:sequence>
			<xs:attributeGroup ref="fi:statusEnt" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VMERef">
		<xs:annotation>
			<xs:documentation>
				<Title>VME reference</Title>
				<Description> Element used to Refer to a VME record.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:FigisID" maxOccurs="1" />
				<xs:element ref="dc:Title" maxOccurs="6" />
				<xs:element ref="fi:ForeignID" maxOccurs="unbounded" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="VMEType">
		<xs:annotation>
			<xs:documentation>
				<Title>VME Type</Title>
				<Description>XXXX</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="required" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Volume">
		<xs:annotation>
			<xs:documentation>
				<Title>Volume</Title>
				<Description>contains a measurement. It may be by weight or by volume (Space). Define the correct fi:Unit attribute
					and tag the value. Use Scale
					if necessary, e.g. if the text reads '20 million' tag '20' as a value and insert
					'million' in the Scale attribute.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attributeGroup ref="fi:volumeUnit" />
			<xs:attributeGroup ref="fi:monetaryScale" />
			<xs:attribute name="Value" type="xs:positiveInteger" />
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VolumeTitle">
		<xs:annotation>
			<xs:documentation>
				<Title>Volume Title</Title>
				<Description>VolumeTitle contains the name or number of a volume.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="WaterAreaList">
		<xs:annotation>
			<xs:documentation>
				<Title>Water Area List</Title>
				<Description>contains a group of water area references with optionally an ad hoc title for the corresponding group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="dc:Title" />
				<xs:element ref="fi:WaterAreaRef" />
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="WaterAreaOverview">
		<xs:annotation>
			<xs:documentation>
				<Title>Water area overview</Title>
				<Description>"Description of the water area in which the aquatic resource is located, including environmental
					characteristics, jurisdictional
					limits, ..." (FIGIS-FIRMS 2002 workshop). Water Area Overview might contain a
					reference list of areas in which a species or aquatic resource is
					found or employed.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="fi:SpatialScale" minOccurs="0" />
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="fi:WaterAreaRef" />
					<xs:element ref="fi:History" />
					<xs:group ref="fi:textEnt" />
				</xs:choice>
			</xs:sequence>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="WaterAreaRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Water area reference</Title>
				<Description>Element used to refer to a Water Area domain element. It contains water geographic systems and codes
					(FAO Area, Five Degree Square,
					ISO country, regional/local codes, water competence zones and environments, ...) used
					for positive referencing of a water area.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID" />
					<xs:element ref="fi:ForeignID" />
				</xs:choice>
				<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6" />
				<xs:element ref="fi:IntersectingAreas" minOccurs="0" />
				<xs:element ref="fi:AdditionalRefData" minOccurs="0" />
				<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Environmental" />
						<xs:enumeration value="Statistical" />
						<xs:enumeration value="Competence" />
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="WaterCondition">
		<xs:annotation>
			<xs:documentation>
				<Title>Water Conditions</Title>
				<Description>WaterCondition is information on species affinity related to water conditions or characteristics , e.g.
					oxygen , temperature etc.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="WaterQuality">
		<xs:annotation>
			<xs:documentation>
				<Title>WaterQuality</Title>
				<Description>contains information about WaterQuality for aquaculture.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt" />
				<xs:element ref="fi:License" />
				<xs:element ref="fi:OrgsInvolved" />
				<xs:element ref="fi:TermsAndConditions" />
				<xs:element ref="fi:Procedures" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Website">
		<xs:annotation>
			<xs:documentation>
				<Title>Website</Title>
				<Description>Website contains the WWW ADDRESS for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute ref="xml:lang" />
			<xs:attribute name="Label" type="xs:string" />
		</xs:complexType>
	</xs:element>
	<xs:element name="Weight">
		<xs:annotation>
			<xs:documentation>
				<Title>Weight</Title>
				<Description>Weight is a container for information directly related to the weight of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="fi:Min" />
				<xs:element ref="fi:Max" />
			</xs:choice>
			<xs:attribute ref="xml:lang" />
