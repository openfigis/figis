<?xml version="1.0"?><FigisProperties>	<!-- properties of ConnectionMaint class -->	<!-- lifetime of an unused physical connection (sec) -->	<!-- maximum number of open connections -->	<!-- and log level (obsolete) -->	<ConnectionMaint>		<LifeTime>120</LifeTime>		<MaxOpenConnections>4</MaxOpenConnections>		<LogLevel>2</LogLevel>	</ConnectionMaint>	<!-- properties of FAOContextFactory class -->	<!-- lifetime of an unused context(sec) -->	<!-- and maximum number of open connections -->	<FAOContextFactory>		<LifeTime>1800</LifeTime>		<LogLevel>2</LogLevel>	</FAOContextFactory>	<!-- properties of XSL processor -->	<Xsl>		<!-- this is the Xalan-Java processor -->		<Processor Name="XALAN_J">			<Class>org.fao.fi.figis.util.xsl.XslProcAccessLocal</Class>		</Processor>		<LogLevel>2</LogLevel>	</Xsl></FigisProperties>