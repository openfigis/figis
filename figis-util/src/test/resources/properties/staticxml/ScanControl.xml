<StaticXmlScanControl defaultstatus="3">	<!-- AREA -->	<ScanArea directory="wwwroot/fi/figis/area/data" scansubdirs="n" ds="area" statustag="/fi:FIGISDoc/fi:Area/fi:AreaIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:Area/fi:AreaIdent/fi:Name"/>		</Title>		<Parameter name="dom" value="area"/>	</ScanArea>	<!-- COLLECTION -->	<ScanArea directory="wwwroot/fi/figis/collection/data" scansubdirs="n" ds="collection" statustag="/fi:FIGISDoc/fi:Collection/fi:CollectionIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:ObjectSource/fi:CoverPage/dc:Title"/>			<Separator value=": "/>			<Element tag="/fi:FIGISDoc/fi:Collection/fi:CollectionIdent/dc:Title"/>		</Title>		<Parameter name="dom" value="collection"/>	</ScanArea>	<!-- COUNTRY SECTOR -->	<ScanArea directory="wwwroot/fi/figis/countrysector/data" scansubdirs="n" ds="countrysector" statustag="/fi:FIGISDoc/fi:CountrySector/fi:CountrySectorIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:ObjectSource/fi:CoverPage/dc:Title"/>		</Title>		<Parameter name="dom" value="countrysector"/>	</ScanArea>	<!-- CULTURE SPECIES -->	<ScanArea directory="wwwroot/fi/figis/culturespecies/data" invpattern=".*labels\.xml" scansubdirs="n" ds="culturespecies" statustag="/fi:FIGISDoc/fi:CultureSpecies/fi:CultureSpeciesIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:CultureSpecies/fi:CultureSpeciesIdent/fi:SpeciesRef/dc:Title"/>		</Title>		<Parameter name="dom" value="culturespecies"/>	</ScanArea>	<!-- EQUIPMENT -->	<ScanArea directory="wwwroot/fi/figis/equipment/data" scansubdirs="n" ds="equipment" statustag="/fi:FIGISDoc/fi:FishEquip/fi:FishEquipIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:FishEquip/fi:FishEquipIdent/dc:Title"/>		</Title>		<Parameter name="dom" value="equipment"/>	</ScanArea>	<!-- LEGAL FRAMEWORK -->	<ScanArea directory="wwwroot/fi/figis/legalframework/data" scansubdirs="n" ds="legalframework" statustag="/fi:FIGISDoc/fi:LegalFramework/fi:LegalFrameworkIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:LegalFramework/fi:LegalFrameworkIdent/dc:Identifier"/>		</Title>		<Parameter name="dom" value="legalframework"/>	</ScanArea>	<!-- ONTOLOGY -->	<ScanArea directory="wwwroot/fi/figis/ontology/data" scansubdirs="n" ds="ontology" statustag="/fi:FIGISDoc/fi:Ontology/fi:OntologyIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:ObjectSource/fi:CoverPage/dc:Title/fi:ChapterTitle"/>		</Title>		<Parameter name="dom" value="ontology"/>	</ScanArea>	<!-- VMSPROGRAMME -->	<ScanArea directory="wwwroot/fi/figis/vmsprogramme/data" scansubdirs="n" ds="vmsprogramme" statustag="/fi:FIGISDoc/fi:VMSProgramme/fi:VMSProgrammeIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:ObjectSource/fi:CoverPage/dc:Title"/>		</Title>		<Parameter name="dom" value="vmsprogramme"/>	</ScanArea>	<!-- VMSTOPIC -->	<ScanArea directory="wwwroot/fi/figis/topic/data" scansubdirs="n" ds="vmstopic">		<Title>			<Element tag="/fi:FIGISDoc/fi:Topic/fi:TopicIdent/dc:Title"/>		</Title>		<Parameter name="dom" value="vmstopic"/>	</ScanArea>	<!-- ORG -->	<ScanArea directory="wwwroot/fi/figis/org/data" scansubdirs="n" ds="org" statustag="/fi:FIGISDoc/fi:Org/fi:OrgIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:Org/fi:OrgIdent/dc:Title"/>		</Title>		<Parameter name="dom" value="org"/>	</ScanArea>	<!-- Species Group -->	<ScanArea directory="wwwroot/fi/figis/speciesgroup/data" scansubdirs="n" ds="org">		<Title>			<Element tag="/fi:FIGISDoc/fint:WebPage/tunalike-kw/name"/>		</Title>		<Parameter name="dom" value="speciesgroup"/>	</ScanArea>	<!--PSM -->	<ScanArea directory="wwwroot/fi/figis/psm/data" scansubdirs="n" ds="psm" statustag="/fi:FIGISDoc/fi:PortStateMeasure/fi:PortStateMeasureIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:PortStateMeasure/fi:PortStateMeasureIdent/dc:Title"/>		</Title>		<Parameter name="dom" value="psm"/>	</ScanArea>	<!--EAFTOOL -->	<ScanArea directory="wwwroot/fi/figis/eaftool/data" scansubdirs="n" ds="eaftool" statustag="/fi:FIGISDoc/fi:EAFTool/fi:EAFToolIdent@Status">		<Title>			<Element tag="/fi:FIGISDoc/fi:EAFTool/fi:EAFToolIdent/dc:Title"/>		</Title>		<Parameter name="dom" value="eaftool"/>	</ScanArea></StaticXmlScanControl>