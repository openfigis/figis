<?xml version="1.0" encoding="UTF-8"?><!-- XSL identification Header***********************************************************************************************	*na- MarineResTemplate.xsl																	***********************************************************************************************	*ti-  Main Fact sheets template ***********************************************************************************************	*md- Marine Resources																		*sm-																					***********************************************************************************************	*ds- Handle structure of the XML displayed.***********************************************************************************************	*ac-																					***********************************************************************************************	*hi- 02-aug-2005	Created By Yann Laurent				***********************************************************************************************	--><!-- Create a DTD/schema --><FSTemplate dataset="MarineResource">	<!-- FSLabel: by default Fact Sheet -->	<htmlHeadLabel>		<label xml:lang="en">FIRMS Marine Resource Fact Sheet</label>		<label xml:lang="fr">Page de données pour les ressouces marines de FIRMS</label>		<label xml:lang="es">FIRMS Marine Resource Fact Sheet</label>	</htmlHeadLabel>	<!--<htmlHeadLabelObs>		<label xml:lang="en">FIRMS Marine Resource Observation Sheet</label>		<label xml:lang="fr">Page de données pour les ressouces marines de FIRMS</label>		<label xml:lang="es">FIRMS Marine Resource Observation Sheet</label>	</htmlHeadLabelObs>-->	<table class="MainDataTable" width="650" cellspacing="0" cellpadding="0" border="0">		<tr>			<tr>				<td width="60%" align="left">					<FactSheetTitle/>				</td>				<td width="40%" align="right">					<toolbar/>				</td>			</tr>			<td colspan="2">				<FrameTitle/>			</td>		</tr>		<tr>			<td colspan="2">				<ownership/>			</td>		</tr>		<tr>			<td colspan="2">				<!-- Display IdentBlock -->				<table width="100%" class="identBlock">					<tr>						<td>							<identBlock/>						</td>					</tr>				</table>			</td>		</tr>		<tr>			<td colspan="2">				<!-- Display data -->				<table width="100%" class="DisplayData">					<tr>						<td>							<contentData/>						</td>					</tr>					<tr>						<td>							<tableSummary/>						</td>					</tr>				</table>			</td>		</tr>	</table></FSTemplate>