<?xml version="1.0" encoding="UTF-8"?><!-- XSL identification Header***********************************************************************************************	*na- FisheryTemplate.xsl																	***********************************************************************************************	*ti-  Main Fact sheets template ***********************************************************************************************	*md- Fishery																		*sm-																					***********************************************************************************************	*ds- Handle structure of the XML displayed.***********************************************************************************************	*ac-																					***********************************************************************************************	*hi- 04-Jul-2006	Created By Sylvain Caillot				***********************************************************************************************	--><!-- Create a DTD/schema --><FSTemplate dataset="Fishery">	<!-- FSLabel: by default Fact Sheet -->	<htmlHeadLabel>		<label xml:lang="en">FIRMS Fishery Fact Sheet</label>		<label xml:lang="fr">Page de donn√©es pour les pecheries de FIRMS</label>		<label xml:lang="es">FIRMS Fishery Fact Sheet</label>	</htmlHeadLabel>	<table class="MainDataTable" width="100%" cellspacing="0" cellpadding="0" border="0">		<tr>			<tr>				<td width="60%" align="left">					<FactSheetTitle/>				</td>				<td width="40%" align="right">					<toolbar/>				</td>			</tr>			<td colspan="2">				<FrameTitle/>			</td>		</tr>		<tr>			<td colspan="2">							<ownership/>			</td>		</tr>		<tr>			<td colspan="2">				<!-- Display IdentBlock -->				<identBlock/>			</td>		</tr>		<tr>			<td colspan="2">				<!-- Display data -->				<div>					<contentData/>				</div>			</td>		</tr>	</table></FSTemplate>