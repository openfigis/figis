			<dc:Title>Inter-American Tropical Tuna Commission (IATTC)</dc:Title>
			<fi:Image IID="19541" Src="iattc_logo.jpg" Title="IATTC Logo" Use="Normal" />
			<fi:StateOfActivity Type="Ongoing" />
